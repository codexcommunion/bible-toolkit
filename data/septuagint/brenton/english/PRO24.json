[
    {
        "book_title": "Brenton Septuagint Translation Proverbs 24",
        "book_usfm_code": "PRO",
        "chapter_number": 24,
        "verse_number": 1,
        "verse_text": "My son, envy not bad men, nor desire to be with them.",
