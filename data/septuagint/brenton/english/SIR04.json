    },
    {
        "book_title": "Brenton Septuagint Translation Sirach 4",
        "book_usfm_code": "SIR",
        "chapter_number": 4,
        "verse_number": 15,
        "verse_text": "Whoso giveth ear unto her shall judge the nations: and he that attendeth unto her shall dwell securely.",
        "iso-639-2_language_code": "eng"
    },
    {
        "book_title": "Brenton Septuagint Translation Sirach 4",
        "book_usfm_code": "SIR",
        "chapter_number": 4,
        "verse_number": 16,
