        "book_title": "Brenton Septuagint Translation Job 39",
        "book_usfm_code": "JOB",
        "chapter_number": 39,
        "verse_number": 23,
        "verse_text": "The bow and sword resound against him; and his rage will swallow up the ground:",
        "iso-639-2_language_code": "eng"
    },
