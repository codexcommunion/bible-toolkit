    },
    {
        "book_title": "Brenton Septuagint Translation Sirach 8",
        "book_usfm_code": "SIR",
        "chapter_number": 8,
        "verse_number": 19,
        "verse_text": "Open not thine heart to every man, lest he requite thee with a shrewd turn.",
        "iso-639-2_language_code": "eng"
