[
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 1,
        "verse_text": "Κρείσσων ἀνὴρ ἐλέγχων ἀνδρὸς σκληροτραχήλου, ἐξαπίνης γὰρ φλεγομένου αὐτοῦ οὐκ ἔστιν ἴασις.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 2,
        "verse_text": "Ἐγκωμιαζομένων δικαίων εὐφρανθήσονται λαοὶ, ἀρχόντων δὲ ἀσεβῶν στένουσιν ἄνδρες.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 3,
        "verse_text": "Ἀνδρὸς φιλοῦντος σοφίαν εὐφραίνεται πατὴρ αὐτοῦ, ὃς δὲ ποιμαίνει πόρνας ἀπολεῖ πλοῦτον.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 4,
        "verse_text": "Βασιλεὺς δίκαιος ἀνίστησι χώραν, ἀνὴρ δὲ παράνομος κατασκάπτει.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 5,
        "verse_text": "Ὃς παρασκευάζεται ἐπὶ πρόσωπον τοῦ ἑαυτοῦ φιλου δίκτυον, περιβάλλει αὐτὸ τοῖς ἑαυτοῦ ποσίν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 6,
        "verse_text": "Ἁμαρτάνοντι ἀνδρὶ μεγάλη παγὶς, δίκαιος δὲ ἐν χαρᾷ καὶ ἐν εὐφροσύνῃ ἔσται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 7,
        "verse_text": "Ἐπίσταται δίκαιος κρίνειν πενιχροῖς, ὁ δὲ ἀσεβὴς οὐ νοεῖ γνῶσιν, καὶ πτωχῷ οὐχ ὑπάρχει νοῦς ἐπιγνώμων.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 8,
        "verse_text": "Ἄνδρες ἄνομοι ἐξέκαυσαν πόλιν, σοφοὶ δὲ ἀπέστρεψαν ὀργήν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 9,
        "verse_text": "Ἀνὴρ σοφὸς κρινεῖ ἔθνη, ἀνὴρ δὲ φαῦλος ὀργιζόμενος καταγελᾶται καὶ οὐ καταπτήσσει.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 10,
        "verse_text": "Ἄνδρες αἱμάτων μέτοχοι μισοῦσιν ὅσιον, οἱ δὲ εὐθεῖς ἐκζητήσουσι ψυχὴν αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 11,
        "verse_text": "Ὅλον τὸν θυμὸν αὐτοῦ ἐκφέρει ἄφρων, σοφὸς δὲ ταμιεύεται κατὰ μέρος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 12,
        "verse_text": "Βασιλέως ὑπακούοντος λόγον ἄδικον, πάντες οἱ ὑπʼ αὐτὸν παράνομοι.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 13,
        "verse_text": "Δανιστοῦ καὶ χρεωφειλέτου ἀλλήλοις συνελθόντων, ἐπισκοπὴν ἀμφοτέρων ποιεῖται ὁ Κύριος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 14,
        "verse_text": "Βασιλέως ἐν ἀληθείᾳ κρίνοντος πτωχοὺς, ὁ θρόνος αὐτοῦ εἰς μαρτύριον κατασταθήσεται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 15,
        "verse_text": "Πληγαὶ καὶ ἔλεγχοι διδόασι σοφίαν, παῖς δὲ πλανώμενος αἰσχύνει γονεῖς αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 16,
        "verse_text": "Πολλῶν ὄντων ἀσεβῶν πολλαὶγίνονται ἁμαρτίαι, οἱ δὲ δίκαιοι ἐκείνων πιπτόντων κατάφοβοι γίνονται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 17,
        "verse_text": "Παίδευε υἱόν σου, καὶ ἀναπαύσει σε, καὶ δώσει κόσμον τῇ ψυχῇ σου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 18,
        "verse_text": "Οὐ μὴ ὑπάρξῃ ἐξηγητὴς ἔθνει παρανόμῳ, ὁ δὲ φυλάσσων τὸν νόμον μακαριστός.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 19,
        "verse_text": "Λόγοις οὐ παιδευθήσεται οἰκέτης σκληρός· ἐὰν γὰρ καὶ νοήσῃ, ἀλλʼ οὐχ ὑπακούσεται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 20,
        "verse_text": "Ἐὰν ἴδῃς ἄνδρα ταχὺν ἐν λόγοις, γίνωσκε ὅτι ἐλπίδα ἔχει μᾶλλον ὁ ἄφρων αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 21,
        "verse_text": "Ὃς κατασπαταλᾷ ἐκ παιδὸς, οἰκέτης ἔσται, ἔσχατον δὲ ὀδυνηθήσεται ἐφʼ ἑαυτῷ·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 22,
        "verse_text": "Ἀνὴρ θυμώδης ἐγείρει νεῖκος, ἀνὴρ δὲ ὀργίλος ἐξώρυξεν ἁμαρτίαν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 23,
        "verse_text": "Ὕβρις ἄνδρα ταπεινοῖ, τοὺς δὲ ταπεινόφρονας ἐρείδει δόξῃ Κύριος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 24,
        "verse_text": "Ὃς μερίζεται κλέπτῃ, μισεῖ τὴν ἑαυτοῦ ψυχήν· ἐὰν δὲ ὅρκου προτεθέντος ἀκούσαντες μὴ ἀναγγείλωσι,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 25,
        "verse_text": "φοβηθέντες καὶ αἰσχυνθέντες ἀνθρώπους ὑπεσκελίσθησαν, ὁ δὲ πεποιθὼς ἐπὶ Κυρίῳ εὐφρανθήσεται· ἀσέβεια ἀνδρὶ δίδωσι σφάλμα, ὃς δὲ πέποιθεν ἐπὶ τῷ δεσπότῃ σωθήσεται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 26,
        "verse_text": "Πολλοὶ θεραπεύουσι πρόσωπα ἡγουμένων, παρὰ δὲ Κυρίου γίνεται τὸ δίκαιον ἀνδρί.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 29",
        "book_usfm_code": "PRO",
        "chapter_number": 29,
        "verse_number": 27,
        "verse_text": "Βδέλυγμα δίκαιος ἀνὴρ ἀνδρὶ ἀδίκῳ, βδέλυγμα δὲ ἀνόμῳ κατευθύνουσα ὁδός.",
        "iso-639-2_language_code": "grc"
    }
]