[
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 1,
        "verse_text": "Αἴνεσις τοῦ Δαυίδ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 2,
        "verse_text": "Καθʼ ἑκάστην ἡμέραν εὐλογήσω σε, καὶ αἰνέσω τὸ ὄνομά σου εἰς τὸν αἰῶνα καὶ εἰς τὸν αἰῶνα τοῦ αἰῶνος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 3,
        "verse_text": "Μέγας ὁ Κύριος καὶ αἰνετὸς σφόδρα, καὶ τῆς μεγαλωσύνης αὐτοῦ οὐκ ἔστι πέρας.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 4,
        "verse_text": "Γενεὰ καὶ γενεὰ ἐπαινέσει τὰ ἔργα σου, καὶ τὴν δύναμίν σου ἀπαγγελοῦσι.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 5,
        "verse_text": "Καὶ τὴν μεγαλοπρέπειαν τῆς δόξης τῆς ἁγιωσύνης σου λαλήσουσι, καὶ τὰ θαυμάσιά σου διηγήσονται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 6,
        "verse_text": "Καὶ τὴν δύναμιν τῶν φοβερῶν σου ἐροῦσι, καὶ τὴν μεγαλωσύνην σου διηγήσονται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 7,
        "verse_text": "Μνήμην τοῦ πλήθους τῆς χρηστότητός σου ἐξερεύξονται, καὶ τῇ δικαιοσύνῃ σου ἀγαλλιάσονται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 8,
        "verse_text": "Οἰκτίρμων καὶ ἐλεήμων ὁ Κύριος, μακρόθυμος καὶ πολυέλεος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 9,
        "verse_text": "Χρηστὸς Κύριος τοῖς ὑπομένουσι, καὶ οἱ οἰκτιρμοὶ αὐτοῦ ἐπὶ πάντα τὰ ἔργα αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 10,
        "verse_text": "Ἐξομολογησάσθωσάν σοι, Κύριε, πάντα τὰ ἔργα σου, καὶ οἱ ὅσιοί σου εὐλογησάτωσάν σε.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 11,
        "verse_text": "Δόξαν τῆς βασιλείας σου ἐροῦσι, καὶ τὴν δυναστείαν σου λαλήσουσιν·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 12,
        "verse_text": "Τοῦ γνωρίσαι τοῖς υἱοῖς τῶν ἀνθρώπων τὴν δυναστείαν σου, καὶ τὴν δόξαν τῆς μεγαλοπρεπείας τῆς βασιλείας σου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 13,
        "verse_text": "Ἡ βασιλεία σου βασιλεία πάντων τῶν αἰώνων, καὶ ἡ δεσποτία σου ἐν πάσῃ γενεᾷ καὶ γενεᾷ·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 14,
        "verse_text": "πιστὸς Κύριος ἐν τοῖς λόγοις αὐτοῦ, καὶ ὅσιος ἐν πᾶσι τοῖς ἔργοις αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 14,
        "verse_text": "Ὑποστηρίζει Κύριος πάντας τοὺς καταπίπτοντας, καὶ ἀνορθοῖ πάντας τοὺς κατεῤῥαγμένους.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 15,
        "verse_text": "Οἱ ὀφθαλμοὶ πάντων εἰς σὲ ἐλπίζουσι, καὶ σὺ δίδως τὴν τροφὴν αὐτῶν ἐν εὐκαιρίᾳ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 16,
        "verse_text": "Ἀνοίγεις σὺ τὰς χεῖράς σου, καὶ ἐμπιπλᾷς πᾶν ζῶον εὐδοκίας.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 17,
        "verse_text": "Δίκαιος Κύριος ἐν πάσαις ταῖς ὁδοῖς αὐτοῦ, καὶ ὅσιος ἐν πᾶσι τοῖς ἔργοις αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 18,
        "verse_text": "Ἐγγὺς Κύριος πᾶσιν τοῖς ἐπικαλουμένοις αὐτόν, πᾶσι τοῖς ἐπικαλουμένοις αὐτὸν ἐν ἀληθείᾳ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 19,
        "verse_text": "Θέλημα τῶν φοβουμένων αὐτὸν ποιήσει, καὶ τῆς δεήσεως αὐτῶν ἐπακούσεται, καὶ σώσει αὐτούς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 20,
        "verse_text": "Φυλάσσει Κύριος πάντας τοὺς ἀγαπῶντας αὐτόν, καὶ πάντας τοὺς ἁμαρτωλοὺς ἐξολοθρεύσει.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 144",
        "book_usfm_code": "PSA",
        "chapter_number": 144,
        "verse_number": 21,
        "verse_text": "Αἴνεσιν Κυρίου λαλήσει τὸ στόμα μου, καὶ εὐλογείτω πᾶσα σὰρξ τὸ ὄνομα τὸ ἅγιον αὐτοῦ, εἰς τὸν αἰῶνα καὶ εἰς τὸν αἰῶνα τοῦ αἰῶνος.",
        "iso-639-2_language_code": "grc"
    }
]