[
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 131",
        "book_usfm_code": "PSA",
        "chapter_number": 131,
        "verse_number": 1,
        "verse_text": "Ὠδὴ τῶν ἀναβαθμῶν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 131",
        "book_usfm_code": "PSA",
        "chapter_number": 131,
        "verse_number": 2,
        "verse_text": "Ὡς ὤμοσε τῷ Κυρίῳ, ηὔξατο τῷ Θεῷ Ἰακώβ·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 131",
        "book_usfm_code": "PSA",
        "chapter_number": 131,
        "verse_number": 3,
        "verse_text": "Εἰ εἰσελεύσομαι εἰς σκήνωμα οἴκου μου, εἰ ἀναβήσομαι ἐπὶ κλίνης στρωμνῆς μου·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 131",
        "book_usfm_code": "PSA",
        "chapter_number": 131,
        "verse_number": 4,
        "verse_text": "Εἰ δώσω ὕπνον τοῖς ὀφθαλμοῖς μοῦ, καὶ τοῖς βλεφάροις μου νυσταγμὸν, καὶ ἀνάπαυσιν τοῖς κροτάφοις μου·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 131",
        "book_usfm_code": "PSA",
        "chapter_number": 131,
        "verse_number": 5,
        "verse_text": "Ἕως οὗ εὕρω τόπον τῷ Κυρίῳ, σκήνωμα τῷ Θεῷ Ἰακώβ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 131",
        "book_usfm_code": "PSA",
        "chapter_number": 131,
        "verse_number": 6,
        "verse_text": "Ἰδοὺ ἠκούσαμεν αὐτὴν ἐν Ἐφραθὰ, εὕρομεν αὐτὴν ἐν τοῖς πεδίοις τοῦ δρυμοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 131",
        "book_usfm_code": "PSA",
        "chapter_number": 131,
        "verse_number": 7,
        "verse_text": "Εἰσελευσώμεθα εἰς τὰ σκηνώματα αὐτοῦ· προσκυνήσωμεν εἰς τὸν τόπον οὗ ἔστησαν οἱ πόδες αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 131",
        "book_usfm_code": "PSA",
        "chapter_number": 131,
        "verse_number": 8,
        "verse_text": "Ἀνάστηθι Κύριε εἰς τὴν ἀνάπαυσίν σου, σὺ καὶ ἡ κιβωτὸς τοῦ ἁγιάσματός σου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 131",
        "book_usfm_code": "PSA",
        "chapter_number": 131,
        "verse_number": 9,
        "verse_text": "Οἱ ἱερεῖς σου ἐνδύσονται δικαιοσύνην, καὶ οἱ ὅσιοί σου ἀγαλλιάσονται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 131",
        "book_usfm_code": "PSA",
        "chapter_number": 131,
        "verse_number": 10,
        "verse_text": "Ἕνεκεν Δαυὶδ τοῦ δούλου σου, μὴ ἀποστρέψῃς τὸ πρόσωπον τοῦ χριστοῦ σου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 131",
        "book_usfm_code": "PSA",
        "chapter_number": 131,
        "verse_number": 11,
        "verse_text": "Ὤμοσε Κύριος τῷ Δαυὶδ ἀλήθειαν, καὶ οὐ μὴ ἀθετήσει αὐτὴν, ἐκ καρποῦ τῆς κοιλίας σου, θήσομαι ἐπὶ τοῦ θρόνον σου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 131",
        "book_usfm_code": "PSA",
        "chapter_number": 131,
        "verse_number": 12,
        "verse_text": "Ἐὰν φυλάξωνται οἱ υἱοί σου τὴν διαθήκην μου, καὶ τὰ μαρτύριά μου ταῦτα ἃ διδάξω αὐτοὺς, καὶ οἱ υἱοὶ αὐτῶν ἕως τοῦ αἰῶνος καθιοῦνται ἐπὶ τοῦ θρόνου σου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 131",
        "book_usfm_code": "PSA",
        "chapter_number": 131,
        "verse_number": 13,
        "verse_text": "Ὅτι ἐξελέξατο Κύριος τὴν Σιὼν, ᾑρετίσατο αὐτὴν εἰς κατοικίαν ἑαυτῷ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 131",
        "book_usfm_code": "PSA",
        "chapter_number": 131,
        "verse_number": 14,
        "verse_text": "Αὕτη ἡ κατάπαυσίς μου εἰς αἰῶνα αἰῶνος, ὧδε κατοικήσω ὅτι ᾑρετισάμην αὐτήν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 131",
        "book_usfm_code": "PSA",
        "chapter_number": 131,
        "verse_number": 15,
        "verse_text": "Τὴν θήραν αὐτῆς εὐλογῶν εὐλογήσω, τοὺς πτωχοὺς αὐτῆς χορτάσω ἄρτων.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 131",
        "book_usfm_code": "PSA",
        "chapter_number": 131,
        "verse_number": 16,
        "verse_text": "Τοὺς ἱερεῖς αὐτῆς ἐνδύσω σωτηρίαν, καὶ οἱ ὅσιοι αὐτῆς ἀγαλλιάσει ἀγαλλιάσονται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 131",
        "book_usfm_code": "PSA",
        "chapter_number": 131,
        "verse_number": 17,
        "verse_text": "Ἐκεῖ ἐξανατελῶ κέρας τῷ Δαυὶδ, ἡτοίμασα λύχνον τῷ χριστῷ σου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 131",
        "book_usfm_code": "PSA",
        "chapter_number": 131,
        "verse_number": 18,
        "verse_text": "Τοὺς ἐχθροὺς αὐτοῦ ἐνδύσω αἰσχύνην, ἐπὶ δὲ αὐτὸν ἐξανθήσει τὸ ἁγίασμά μου.",
        "iso-639-2_language_code": "grc"
    }
]