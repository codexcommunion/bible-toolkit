[
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "1",
        "text": "Προφάσεις ζητεῖ ἀνὴρ βουλόμενος χωρίζεσθαι ἀπὸ φίλων, ἐν παντὶ δὲ καιρῷ ἐπονείδιστος ἔσται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "2",
        "text": "Οὐ χρείαν ἔχει σοφίας ἐνδεὴς φρενῶν, μᾶλλον γᾶρ ἄγεται ἀφροσύνῃ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "3",
        "text": "Ὅταν ἔλθῃ ἀσεβὴς εἰς βάθος κακῶν, καταφρονεῖ, ἐπέρχεται δὲ αὐτῷ ἀτιμία καὶ ὄνειδος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "4",
        "text": "Ὕδωρ βαθὺ λόγος ἐν καρδίᾳ ἀνδρὸς, ποταμὸς δὲ ἀναπηδύει καὶ πηγὴ ζωῆς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "5",
        "text": "Θαυμάσαι πρόσωπον ἀσεβοῦς οὐ καλὸν, οὐδὲ ὅσιον ἐκκλίνειν τὸ δίκαιον ἐν κρίσει.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "6",
        "text": "Χείλη ἄφρονος ἄγουσιν αὐτὸν εἰς κακὰ, τὸ δὲ στόμα αὐτοῦ τὸ θρασὺ θάνατον ἐπικαλεῖται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "7",
        "text": "Στόμα ἄφρονος συντριβὴ αὐτῷ, τὰ δὲ χείλη αὐτοῦ παγὶς τῇ ψυχῇ αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "8",
        "text": "Ὀκνηροὺς καταβάλλει φόβος, ψυχαὶ δὲ ἀνδρογύνων πεινάσουσιν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "9",
        "text": "Ὁ μὴ ἰώμενος αὐτὸν ἐν τοῖς ἔργοις αὐτοῦ, ἀδελφός ἐστι τοῦ λυμαινομένου ἑαυτόν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "10",
        "text": "Ἐκ μεγαλωσύνης ἰσχύος ὄνομα Κυρίου, αὐτῷ δὲ προσδραμόντες δίκαιοι ὑψοῦνται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "11",
        "text": "Ὕπαρξις πλουσίου ἀνδρὸς πόλις ὀχυρὰ, ἡ δὲ δόξα αὐτῆς μέγα ἐπισκιάζει.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "12",
        "text": "Πρὸ συντριβῆς ὑψοῦται καρδία ἀνδρὸς, καὶ πρὸ δόξης ταπεινοῦνται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "13",
        "text": "Ὃς ἀποκρίνεται λόγον πρὶν ἀκοῦσαι, ἀφροσύνη αὐτῷ ἐστι καὶ ὄνειδος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "14",
        "text": "Θυμὸν ἀνδρὸς πρᾳΰνει θεράπων φρόνιμος, ὀλιγόψυχον δὲ ἄνδρα τίς ὑποίσει;",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "15",
        "text": "Καρδία φρονίμου κτᾶται αἴσθησιν, ὦτα δὲ σοφῶν ζητεῖ ἔννοιαν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "16",
        "text": "Δόμα ἀνθρώπου ἐμπλατύνει αὐτὸν, καὶ παρὰ δυνάσταις καθιζάνει αὐτόν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "17",
        "text": "Δίκαιος ἑαυτοῦ κατήγορος ἐν πρωτολογίᾳ, ὡς δʼ ἂν ἐπιβάλῃ ὁ ἀντίδικος ἐλέγχεται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "18",
        "text": "Ἀντιλογίας παύει σιγηρὸς, ἐν δὲ δυναστείαις ὁρίζει.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "19",
        "text": "Ἀδελφὸς ὑπὸ ἀδελφοῦ βοηθούμενος, ὡς πόλις ὀχυρὰ καὶ ὑψηλὴ, ἰσχύει δὲ ὥσπερ τεθεμελιωμένον βασίλειον.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "20",
        "text": "Ἀπὸ καρπῶν στόματος ἀνὴρ πίμπλησι κοιλίαν αὐτοῦ, ἀπὸ δὲ καρπῶν χειλέων αὐτοῦ ἐμπλησθήσεται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "21",
        "text": "Θάνατος καὶ ζωὴ ἐν χειρὶ γλώσσης, οἱ δὲ κρατοῦντες αὐτῆς ἔδονται τοὺς καρποὺς αὐτῆς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "22",
        "text": "Ὃς εὗρε γυναῖκα ἀγαθὴν, εὗρε χάριτας, ἔλαβε δὲ παρὰ Θεοῦ ἱλαρότητα·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 18",
        "usfm_code": "PRO",
        "chapter": 18,
        "verse": "22a",
        "text": "ὃς ἐκβάλλει γυναῖκα ἀγαθὴν, ἐκβάλλει τὰ ἀγαθὰ, ὁ δὲ κατέχων μοιχαλίδα, ἄφρων καὶ ἀσεβής.",
        "iso-639-2_language_code": "grc"
    }
]