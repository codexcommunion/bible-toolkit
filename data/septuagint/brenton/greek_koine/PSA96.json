    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 96",
        "book_usfm_code": "PSA",
        "chapter_number": 96,
        "verse_number": 8,
        "verse_text": "Ἤκουσε καὶ εὐφράνθη Σιὼν, καὶ ἠγαλλιάσαντο αἱ θυγατέρες τῆς Ἰουδαίας, ἕνεκεν τῶν κριμάτων σου Κύριε.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 96",
