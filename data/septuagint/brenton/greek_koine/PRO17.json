[
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "1",
        "text": "Κρείσσων ψωμὸς μεθʼ ἡδονῆς ἐν εἰρήνῃ, ἢ οἶκος πολλῶν ἀγαθῶν καὶ ἀδίκων θυμάτων μετὰ μάχης.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "2",
        "text": "Οἰκέτης νοήμων κρατήσει δεσποτῶν ἀφρόνων, ἐν δὲ ἀδελφοῖς διελεῖται μέρη.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "3",
        "text": "Ὥσπερ δοκιμάζεται ἐν καμίνῳ ἄργυρος καὶ χρυσὸς, οὕτως ἐκλεκταὶ καρδίαι παρὰ Κυρίῳ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "4",
        "text": "Κακὸς ὑπακούει γλώσσης παρανόμων, δίκαιος δὲ οὐ προσέχει χείλεσι ψευδέσιν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "5",
        "text": "Ὁ καταγελῶν πτωχοῦ παροξύνει τὸν ποιήσαντα αὐτὸν, ὁ δὲ ἐπιχαίρων ἀπολλυμένῳ οὐκ ἀθωωθήσεται, ὁ δὲ ἐπισπλαγχνιζόμενος ἐλεηθήσεται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "6",
        "text": "Στέφανος γερόντων τέκνα τέκνων, καύχημα δὲ τέκνων πατέρες αὐτῶν·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "6a",
        "text": "τοῦ πιστοῦ ὅλος ὁ κόσμος τῶν χρημάτων, τοῦ δὲ ἀπίστου οὐδὲ ὀβολός.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "7",
        "text": "Οὐχ ἁρμόσει ἄφρονι χείλη πιστὰ, οὐδὲ δικαίῳ χείλη ψευδῆ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "8",
        "text": "Μισθὸς χαρίτων παιδεία τοῖς χρωμένοις, οὗ δʼ ἂν ἐπιστρέψῃ εὐοδωθήσεται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "9",
        "text": "Ὃς κρύπτει ἀδικήματα, ζητεῖ φιλίαν· ὃς δὲ μισεῖ κρύπτειν, διΐστησι φίλους καὶ οἰκείους.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "10",
        "text": "Συντρίβει ἀπειλὴ καρδίαν φρονίμου, ἄφρων δὲ μαστιγωθεὶς οὐκ αἰσθάνεται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "11",
        "text": "Ἀντιλογίας ἐγείρει πᾶς κακὸς, ὁ δὲ Κύριος ἄγγελον ἀνελεήμονα ἐκπέμψει αὐτῷ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "12",
        "text": "Ἐμπεσεῖται μέριμνα ἀνδρὶ νοήμονι, οἱ δὲ ἄφρονες διαλογιοῦνται κακά.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "13",
        "text": "Ὃς ἀποδίδωσι κακὰ ἀντὶ ἀγαθῶν, οὐ κινηθήσεται κακὰ ἐκ τοῦ οἴκου αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "14",
        "text": "Ἐξουσίαν δίδωσι λόγοις ἀρχὴ δικαιοσύνης, προηγεῖται δὲ τῆς ἐνδείας στάσις καὶ μάχη.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "15",
        "text": "Ὃς δίκαιον κρίνει τὸν ἄδικον, ἄδικον δὲ τὸν δίκαιον, ἀκάθαρτος καὶ βδελυκτὸς παρὰ Θεῷ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "16",
        "text": "Ἱνατί ὑπῆρξε χρήματα ἄφρονι; κτήσασθαι γὰρ σοφίαν ἀκάρδιος οὐ δυνήσεται·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "16a",
        "text": "ὃς ὑψηλὸν ποιεῖ τὸν ἑαυτοῦ οἶκον, ζητεῖ συντριβήν· ὁ δὲ σκολιάζων τοῦ μαθεῖν, ἐμπεσεῖται εἰς κακά.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "17",
        "text": "Εἰς πάντα καιρὸν φίλος ὑπαρχέτω σοι, ἀδελφοὶ δὲ ἐν ἀνάγκαις χρήσιμοι ἔστωσαν, τούτου γὰρ χάριν γεννῶνται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "18",
        "text": "Ἀνὴρ ἄφρων ἐπικροτεῖ καὶ ἐπιχαίρει ἑαυτῷ, ὡς καὶ ὁ ἐγγυώμενος ἐγγύῃ τῶν ἑαυτοῦ φίλων.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "19",
        "text": "Φιλαμαρτήμων χαίρει μάχαις,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "20",
        "text": "ὁ δὲ σκληροκάρδιος οὐ συναντᾷ ἀγαθοῖς· ἀνὴρ εὐμετάβολος γλώσσῃ ἐμπεσεῖται εἰς κακὰ,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "21",
        "text": "καρδία δὲ ἄφρονος ὀδύνη τῷ κεκτημένῳ αὐτήν· οὐκ εὐφραίνεται πατὴρ ἐφʼ υἱῷ ἀπαιδεύτῳ, υἱὸς δὲ φρόνιμος εὐφραίνει μητέρα αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "22",
        "text": "Καρδία εὐφραινομένη εὐεκτεῖν ποιεῖ, ἀνδρὸς δὲ λυπηροῦ ξηραίνεται τὰ ὀστᾶ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "23",
        "text": "Λαμβάνοντος δῶρα ἀδίκως ἐν κόλποις οὐ κατευοδοῦνται ὁδοὶ, ἀσεβὴς δὲ ἐκκλίνει ὁδοὺς δικαιοσύνης.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "24",
        "text": "Πρόσωπον συνετὸν ἀνδρὸς σοφοῦ, οἱ δὲ ὀφθαλμοὶ τοῦ ἄφρονος ἐπʼ ἄκρα γῆς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "25",
        "text": "Ὀργὴ πατρὶ υἱὸς ἄφρων, καὶ ὀδύνη τῇ τεκούσῃ αὐτόν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "26",
        "text": "Ζημιοῦν ἄνδρα δίκαιον οὐ καλὸν, οὐδὲ ὅσιον ἐπιβουλεύειν δυνάσταις δικαίοις.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "27",
        "text": "Ὃς φείδεται ῥῆμα προέσθαι σκληρὸν, ἐπιγνώμων· μακρόθυμος δὲ ἀνὴρ φρόνιμος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 17",
        "usfm_code": "PRO",
        "chapter": 17,
        "verse": "28",
        "text": "Ἀνοήτῳ ἐπερωτήσαντι σοφίαν σοφία λογισθήσεται, ἐνεὸν δέ τις ἑαυτὸν ποιήσας, δόξει φρόνιμος εἶναι.",
        "iso-639-2_language_code": "grc"
    }
]