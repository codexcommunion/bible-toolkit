    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 45",
        "book_usfm_code": "PSA",
        "chapter_number": 45,
        "verse_number": 10,
        "verse_text": "ἀνταναιρῶν πολέμους μέχρι τῶν περάτων τῆς γῆς, τόξον συντρίψει, καὶ συγκλάσει ὅπλον, καὶ θυρεοὺς κατακαύσει ἐν πυρί.",
        "iso-639-2_language_code": "grc"
