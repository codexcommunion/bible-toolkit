[
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "1",
        "text": "Υἱοὶ Λευὶ, Γεδσὼν, Καὰθ, καὶ Μεραρί.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "2",
        "text": "Καὶ ταῦτα τὰ ὀνόματα τῶν υἱῶν Γεδσὼν, Λοβενὶ, καὶ Σεμεΐ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "3",
        "text": "Υἱοὶ Καὰθ, Ἄμβραμ, καὶ Ἰσσαὰρ, Χεβρὼν, καὶ Ὀζιήλ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "4",
        "text": "Υἱοὶ Μεραρὶ, Μοολὶ, καὶ ὁ Μουσί· καὶ αὗται αἱ πατριαὶ τοῦ Λευὶ κατὰ πατριὰς αὐτῶν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "5",
        "text": "Τῷ Γεδσὼν, τῷ Λοβενὶ υἱῷ αὐτοῦ, Ἰὲθ υἱὸς αὐτοῦ, Ζαμμὰθ υἱὸς αὐτοῦ,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "6",
        "text": "Ἰωὰβ υἱὸς αὐτοῦ, Ἀδδὶ υἱὸς αὐτοῦ, Ζαρὰ υἱὸς αὐτοῦ, Ἰεθρὶ υἱὸς αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "7",
        "text": "Υἱοὶ Καὰθ, Ἀμιναδὰβ υἱὸς αὐτοῦ, Κορὲ υἱὸς αὐτοῦ, Ἀσὴρ υἱὸς αὐτοῦ,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "8",
        "text": "Ἑλκανὰ υἱὸς αὐτοῦ, Ἀβισὰφ υἱὸς αὐτοῦ, Ἀσὴρ υἱὸς αὐτοῦ,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "9",
        "text": "Θαὰθ υἱὸς αὐτοῦ, Οὐριὴλ υἱὸς αὐτοῦ, Ὀζία υἱὸς αὐτοῦ, Σαοὺλ υἱὸς αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "10",
        "text": "Καὶ υἱοὶ Ἑλκανὰ, Ἀμεσσὶ, καὶ Ἀχιμὼθ,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "11",
        "text": "Ἑλκανὰ υἱὸς αὐτοῦ, Σουφὶ υἱὸς αὐτοῦ, Καιναὰθ υἱὸς αὐτοῦ,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "12",
        "text": "Ἐλιὰβ υἱὸς αὐτοῦ, Ιεροβοὰμ υἱὸς αὐτοῦ, Ἑλκανὰ υἱὸς αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "13",
        "text": "Υἱοὶ Σαμουὴλ, ὁ πρωτότοκος Σανὶ, καὶ Ἀβιά.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "14",
        "text": "Υἱοὶ Μεραρὶ, Μοολὶ, Λοβενὶ υἱὸς αὐτοῦ, Σεμεῒ υἱὸς αὐτοῦ, Ὀζὰ υἱὸς αὐτοῦ,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "15",
        "text": "Σαμαὰ υἱὸς αὐτοῦ, Ἀγγία υἱὸς αὐτοῦ, Ἀσαῒας υἱὸς αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "16",
        "text": "Καὶ οὗτοι οὓς κατέστησε Δαυὶδ ἐπὶ χεῖρας ᾀδόντων ἐν οἴκῳ Κυρίου ἐν τῇ καταπαύσει τῆς κιβωτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "17",
        "text": "Καὶ ἦσαν λειτουργοῦντες ἐναντίον τῆς σκηνῆς τοῦ μαρτυρίου ἐν ὀργάνοις, ἕως οὗ ᾠκοδόμησε Σαλωμὼν τὸν οἶκον Κυρίου ἐν Ἱερουσαλήμ· καὶ ἔστησαν κατὰ τὴν κρίσιν αὐτῶν ἐπὶ τὰς λειτουργίας αὐτῶν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "18",
        "text": "Καὶ οὗτοι οἱ ἑστηκότες, καὶ υἱοὶ αὐτῶν ἐκ τῶν υἱῶν τοῦ Καὰθ, Αἰμὰν ὁ ψαλτῳδὸς υἱὸς Ἰωὴλ, υἱοῦ Σαμουὴλ,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "19",
        "text": "υἱοῦ Ἑλκανὰ, υἱοῦ Ἱεροβοὰμ, υἱοῦ Ἐλιὴλ, υἱοῦ Θοοὺ,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "20",
        "text": "υἱοῦ Σοὺφ, υἱοῦ Ἑλκανὰ, υἱοῦ Μαὰθ, υἱοῦ Ἀμαθὶ,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "21",
        "text": "υἱοῦ Ἑλκανὰ, υἱοῦ Ἰωὴλ, υἱοῦ Ἀζαρία, υἱοῦ Σαφανία,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "22",
        "text": "υἱοῦ Θαὰθ, υἱοῦ Ἀσὴρ, υἱοῦ Ἀβιασὰφ, υἱοῦ Κορὲ,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "23",
        "text": "υἱοῦ Ἰσαὰρ, υἱοῦ Καὰθ, υἱοῦ Λευὶ, υἱοῦ Ἰσραήλ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "24",
        "text": "Καὶ ὁ ἀδελφὸς αὐτοῦ Ἀσὰρ ὁ ἑστηκὼς ἐν δεξιᾷ αὐτοῦ· Ἀσὰρ υἱὸς Βαραχία, υἱοῦ Σαμαὰ,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "25",
        "text": "υἱοῦ Μιχαὴλ, υἱοῦ Βαασία, υἱοῦ Μελχία,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "26",
        "text": "υἱοῦ Ἀθανὶ, υἱοῦ Ζααραῒ, υἱοῦ Ἀδαῒ,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "27",
        "text": "υἱοῦ Αἰθὰμ, υἱοῦ Ζαμμὰμ, υἱοῦ Σεμεῒ,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "28",
        "text": "υἱοῦ Ἰεὲθ, υἱοῦ Γεδσὼν, υἱοῦ Λευί.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "29",
        "text": "Καὶ υἱοὶ Μεραρὶ οἱ ἀδελφοὶ αὐτῶν ἐξ ἀριστερῶν· Αἰθὰμ υἱὸς Κισὰ, υἱοῦ Ἀβαῒ, υἱοῦ Μαλῶχ,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "30",
        "text": "υἱοῦ Ἀσεβὶ, υἱοῦ Ἀμεσσία,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "31",
        "text": "υἱοῦ Βανὶ, υἱοῦ Σεμὴρ,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "32",
        "text": "υἱοῦ Μοολὶ, υἱοῦ Μουσὶ, υἱοῦ Μεραρὶ, υἱοῦ Λευί.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "33",
        "text": "Καὶ οἱ ἀδελφοὶ αὐτῶν κατʼ οἴκους πατριῶν αὐτῶν, οἱ Λευῖται οἱ δεδομένοι εἰς πᾶσαν ἐργασίαν λειτουργίας σκηνῆς οἴκου τοῦ Θεοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "34",
        "text": "Καὶ Ἀαρὼν καὶ υἱοὶ αὐτοῦ θυμιῶντες ἐπὶ τὸ θυσιαστήριον τῶν ὁλοκαυτωμάτων, καὶ ἐπὶ τὸ θυσιαστήριον τῶν θυμιαμάτων εἰς πᾶσαν ἐργασίαν ἅγια τῶν ἁγίων, καὶ ἐξιλάσκεσθαι περὶ Ἰσραὴλ, κατὰ πάντα ὅσα ἐνετείλατο Μωυσῆς παῖς τοῦ Θεοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "35",
        "text": "Καὶ οὗτοι υἱοὶ Ἀαρών· Ἐλεάζαρ υἱὸς αὐτοῦ, Φινεὲς υἱὸς αὐτοῦ, Ἀβισοὺ υἱὸς αὐτοῦ,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "36",
        "text": "Βοκκὶ υἱὸς αὐτοῦ, Ὀζὶ υἱὸς αὐτοῦ, Σαραῒα υἱὸς αὐτοῦ,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "37",
        "text": "Μαριὴλ υἱὸς αὐτοῦ, Ἀμαρία υἱὸς αὐτοῦ, Ἀχιτὼβ υἱὸς αὐτοῦ,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "38",
        "text": "Σαδὼκ υἱὸς αὐτοῦ, Ἀχιμάας υἱὸς αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "39",
        "text": "Καὶ αὗται αἱ κατοικίαι αὐτῶν ἐν ταῖς κώμαις αὐτῶν, ἐν τοῖς ὁρίοις αὐτῶν, τοῖς υἱοῖς Ἀαρὼν τῇ πατριᾷ αὐτῶν τοῖς Κααθὶ, ὅτι αὐτοῖς ἐγένετο ὁ κλῆρος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "40",
        "text": "Καὶ ἔδωκαν αὐτοῖς τὴν Χεβρὼν ἐν γῇ Ἰούδα, καὶ τὰ περισπόρια αὐτῆς κύκλῳ αὐτῆς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "41",
        "text": "Καὶ τὰ πεδία τῆς πόλεως, καὶ τὰς κώμας αὐτῆς ἔδωκαν τῷ Χαλὲβ υἱῷ Ἰεφοννή.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "42",
        "text": "Καὶ τοῖς υἱοῖς Ἀαρὼν ἔδωκαν τὰς πόλεις τῶν φυγαδευτηρίων, τὴν Χεβρὼν, καὶ τὴν Λοβνὰ καὶ τὰ περισπόρια αὐτῆς, καὶ τὴν Σελνὰ καὶ τὰ περισπόρια αὐτῆς, καὶ τὴν Ἐσθαμὼ καὶ τὰ περισπόρια αὐτῆς,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "43",
        "text": "καὶ τὴν Ἰεθὰρ καὶ τὰ περισπόρια αὐτῆς, καὶ τὴν Δαβὶρ καὶ τὰ περισπόρια αὐτῆς,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "44",
        "text": "καὶ τὴν Ἀσὰν καὶ τὰ περισπόρια αὐτῆς, καὶ τὴν Βαιθσαμὺς καὶ τὰ περισπόρια αὐτῆς·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "45",
        "text": "Καὶ ἐκ φυλῆς Βενιαμὶν τὴν Γαβαῒ καὶ τὰ περισπόρια αὐτῆς, καὶ τὴν Γαλεμὰθ καὶ τὰ περισπόρια αὐτῆς, καὶ τὴν Ἀναθὼθ καὶ τὰ περισπόρια αὐτῆς· πᾶσαι αἱ πόλεις αὐτῶν τρισκαίδεκα πόλεις κατὰ πατριὰς αὐτῶν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΑΛΕΙΠΟΜΕΝΩΝ Α 6",
        "usfm_code": "1CH",
        "chapter": 6,
        "verse": "46",
        "text": "Καὶ τοῖς υἱοῖς Καὰθ τοῖς καταλοίποις ἐκ τῶν πατριῶν ἐκ τῆς φυλῆς ἐκ τοῦ ἡμίσους φυλῆς Μανασσῆ, κλήρῳ πόλεις δέκα.",
        "iso-639-2_language_code": "grc"
