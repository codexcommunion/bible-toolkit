[
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "1",
        "text": "Ὁ ζῶν εἰς τὸν αἰῶνα ἔκτισε τὰ πάντα κοινῇ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "2",
        "text": "Κύριος μόνος δικαιωθήσεται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "4",
        "text": "Οὐθενὶ ἐξεποίησεν ἐξαγγεῖλαι τὰ ἔργα αὐτοῦ· καὶ τίς ἐξιχνιάσει τὰ μεγαλεῖα αὐτοῦ;",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "5",
        "text": "Κράτος μεγαλωσύνης αὐτοῦ τίς ἐξαριθμήσεται; καὶ τίς προσθήσει ἐκδιηγήσασθαι τὰ ἐλέη αὐτοῦ;",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "6",
        "text": "Οὐκ ἔστιν ἐλαττῶσαι οὐδὲ προσθεῖναι, καὶ οὐκ ἔστιν ἐξιχνιάσαι τὰ θαυμάσια τοῦ Κυρίου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "7",
        "text": "Ὅταν συντελέσῃ ἄνθρωπος τότε ἄρχεται, καὶ ὅταν παύσηται τότε ἀπορηθήσεται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "8",
        "text": "Τί ἄνθρωπος, καὶ τί ἡ χρῆσις αὐτοῦ; τί τὸ ἀγαθὸν αὐτοῦ, καὶ τί τὸ κακὸν αὐτοῦ;",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "9",
        "text": "Ἀριθμὸς ἡμερῶν ἀνθρώπου πολλὰ ἔτη ἑκατόν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "10",
        "text": "Ὡς σταγὼν ὕδατος ἀπὸ θαλάσσης καὶ ψῆφος ἄμμου, οὕτως ὀλίγα ἔτη ἐν ἡμέρᾳ αἰῶνος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "11",
        "text": "Διὰ τοῦτο ἐμακροθύμησε Κύριος ἐπʼ αὐτοῖς, καὶ ἐξέχεεν ἐπʼ αὐτοὺς τὸ ἔλεος αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "12",
        "text": "Εἶδε καὶ ἐπέγνω τὴν καταστροφὴν αὐτῶν ὅτι πονηρὰ, διὰ τοῦτο ἐπλήθυνε τὸν ἐξιλασμὸν αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "13",
        "text": "Ἔλεος ἄνθρώπου ἐπὶ τὸν πλησίον αὐτοῦ, ἔλεος δὲ Κυρίου ἐπὶ πᾶσαν σάρκα, ἐλέγχων καὶ παιδεύων καὶ διδάσκων καὶ ἐπιστρέφων ὡς ποιμὴν τὸ ποίμνιον αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "14",
        "text": "Τοὺς ἐκδεχομένους παιδείαν ἐλεεῖ, καὶ τοὺς κατασπεύδοντας ἐπὶ τὰ κρίματα αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "15",
        "text": "Τέκνον, ἐν ἀγαθοῖς μὴ δῷς μῶμον, καὶ ἐν πάσῃ δόσει λύπην λόγων.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "16",
        "text": "Οὐχὶ καύσωνα ἀναπαύσει δρόσος; οὕτως κρείσσων λόγος ἢ δόσις.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "17",
        "text": "Οὐκ ἰδοὺ λόγος ὑπὲρ δόμα ἀγαθόν; καὶ ἀμφότερα παρὰ ἀνδρὶ κεχαριτωμένῳ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "18",
        "text": "Μωρὸς ἀχαρίστως ὀνειδιεῖ, καὶ δόσις βασκάνου ἐκτήκει ὀφθαλμούς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "19",
        "text": "Πρινὴ λαλῆσαι μάνθανε, καὶ πρὸ ἀῤῥωστίας θεραπεύου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "20",
        "text": "Πρὸ κρίσεως ἐξέταζε σεαυτόν, καὶ ἐν ὥρᾳ ἐπισκοπῆς εὑρήσεις ἐξιλασμόν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "21",
        "text": "Πρὶν ἀῤῥωστῆσαί σε ταπεινώθητι, καὶ ἐν καιρῷ ἁμαρτημάτων δεῖξον ἐπιστροφήν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "22",
        "text": "Μὴ ἐμποδισθῇς τοῦ ἀποδοῦναι εὐχὴν εὐκαίρως, καὶ μὴ μείνῃς ἕως θανάτου δικαιωθῆναι.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "23",
        "text": "Πρὶν εὔξασθαι ἑτοίμασον σεαυτὸν, και μὴ γίνου ὡς ἄνθρωπος πειράζων τὸν Κύριον.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "24",
        "text": "Μνήσθητι θυμοῦ ἐν ἡμέραις τελευτῆς, καὶ καιρὸν ἐκδικήσεως ἐν ἀποστροφῇ προσώπου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "25",
        "text": "Μνήσθητι καιρὸν λιμοῦ ἐν καιρῷ πλησμονῆς, πτωχείαν καὶ ἔνδειαν ἐν ἡμέραις πλούτου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "26",
        "text": "Ἀπὸ πρωΐθεν ἕως ἑσπέρας μεταβάλλει καιρὸς, καὶ πάντα ἐστὶ ταχινὰ ἔναντι Κυρίου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "27",
        "text": "Ἄνθρωπος σοφὸς ἐν παντὶ εὐλαβηθήσεται, καὶ ἐν ἡμέραις ἁμαρτιῶν προσέξει ἀπὸ πλημμελείας·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "28",
        "text": "πᾶς συνετὸς ἔγνω σοφίαν, καὶ τῷ εὑρόντι αὐτὴν δώσει ἐξομολόγησιν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "29",
        "text": "Συνετοὶ ἐν λόγοις καὶ αὐτοὶ ἐσοφίσαντο, καὶ ἀνώμβρησαν παροιμίας ἀκριβεῖς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "30",
        "text": "Ὀπίσω τῶν ἐπιθυμιῶν σου μὴ πορεύου, καὶ ἀπὸ τῶν ὀρέξεών σου κωλύου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "31",
        "text": "Ἐὰν χορηγήσεῃς τῇ ψυχῇ σου εὐδοκίαν ἐπιθυμίας, ποιήσει σε ἐπίχαρμα τῶν ἐχθρῶν σου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "32",
        "text": "Μὴ εὐφραίνου ἐπὶ πολλῇ τρυφῇ, μηδὲ προσδεθῇς συμβολῇ αὐτῆς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΣΕΙΡΑΧ 18",
        "usfm_code": "SIR",
        "chapter": 18,
        "verse": "33",
        "text": "Μὴ γίνου πτωχὸς συμβολοκοπῶν ἐκ δανεισμοῦ, καὶ οὐδέν σοι ἐστὶν ἐν μαρσυπείῳ.",
        "iso-639-2_language_code": "grc"
    }
]