[
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 90",
        "book_usfm_code": "PSA",
        "chapter_number": 90,
        "verse_number": 1,
        "verse_text": "Αἶνος ᾠδῆς τῷ Δαυίδ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 90",
        "book_usfm_code": "PSA",
        "chapter_number": 90,
        "verse_number": 2,
        "verse_text": "Ἐρεῖ τῷ Κυρίῳ, ἀντιλήπτωρ μου εἶ καὶ καταφυγή μου, ὁ Θεός μου, ἐλπιῶ ἐπʼ αὐτόν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 90",
        "book_usfm_code": "PSA",
        "chapter_number": 90,
        "verse_number": 3,
        "verse_text": "Ὅτι αὐτὸς ῥύσεταί σε ἐκ παγίδος θηρευτῶν, ἀπὸ λόγου ταραχώδους.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 90",
        "book_usfm_code": "PSA",
        "chapter_number": 90,
        "verse_number": 4,
        "verse_text": "Ἐν τοῖς μεταφρένοις αὐτοῦ ἐπισκιάσει σοι, καὶ ὑπὸ τὰς πτέρυγας αὐτοῦ ἐλπιεῖς· ὅπλῳ κυκλώσει σε ἡ ἀλήθεια αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 90",
        "book_usfm_code": "PSA",
        "chapter_number": 90,
        "verse_number": 5,
        "verse_text": "Οὐ φοβηθήσῃ ἀπὸ φόβου νυκτερινοῦ, ἀπὸ βέλους πετομένου ἡμέρας,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 90",
        "book_usfm_code": "PSA",
        "chapter_number": 90,
        "verse_number": 6,
        "verse_text": "ἀπὸ πράγματος διαπορευομένου ἐν σκότει, ἀπὸ συμπτώματος καὶ δαιμονίου μεσημβρινοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 90",
        "book_usfm_code": "PSA",
        "chapter_number": 90,
        "verse_number": 7,
        "verse_text": "Πεσεῖται ἐκ τοῦ κλίτους σου χιλιὰς, καὶ μυριὰς ἐκ δεξιῶν σου, πρὸς σὲ δὲ οὐκ ἐγγιεῖ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 90",
        "book_usfm_code": "PSA",
        "chapter_number": 90,
        "verse_number": 8,
        "verse_text": "Πλὴν τοῖς ὀφθαλμοῖς σου κατανοήσεις, καὶ ἀνταπόδοσιν ἁμαρτωλῶν ὄψει.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 90",
        "book_usfm_code": "PSA",
        "chapter_number": 90,
        "verse_number": 9,
        "verse_text": "Ὅτι σὺ Κύριε ἡ ἐλπίς μου, τὸν ὕψιστον ἔθου καταφυγήν σου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 90",
        "book_usfm_code": "PSA",
        "chapter_number": 90,
        "verse_number": 10,
        "verse_text": "Οὐ προσελεύσεται πρὸς σὲ κακὰ, καὶ μάστιξ οὐκ ἐγγιεῖ τῷ σκηνώματί σου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 90",
        "book_usfm_code": "PSA",
        "chapter_number": 90,
        "verse_number": 11,
        "verse_text": "Ὅτι τοῖς ἀγγέλοις αὐτοῦ ἐντελεῖται περὶ σοῦ, τοῦ διαφυλάξαι σε ἐν πάσαις ταῖς ὁδοῖς σου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 90",
        "book_usfm_code": "PSA",
        "chapter_number": 90,
        "verse_number": 12,
        "verse_text": "Ἐπὶ χειρῶν ἀροῦσί σε, μή ποτε προσκόψῃς πρὸς λίθον τὸν πόδα σου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 90",
        "book_usfm_code": "PSA",
        "chapter_number": 90,
        "verse_number": 13,
        "verse_text": "Ἐπʼ ἀσπίδα καὶ βασιλίσκον ἐπιβήσῃ, καὶ καταπατήσεις λέοντα καὶ δράκοντα.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 90",
        "book_usfm_code": "PSA",
        "chapter_number": 90,
        "verse_number": 14,
        "verse_text": "Ὅτι ἐπʼ ἐμὲ ἤλπισε, καὶ ῥύσομαι αὐτόν· σκεπάσω αὐτὸν, ὅτι ἔγνω τὸ ὄνομά μου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 90",
        "book_usfm_code": "PSA",
        "chapter_number": 90,
        "verse_number": 15,
        "verse_text": "Ἐπικαλέσεται πρὸς μὲ, καὶ εἰσακούσομαι αὐτοῦ, μετʼ αὐτοῦ εἰμι ἐν θλίψει, καὶ ἐξελοῦμαι αὐτὸν, καὶ δοξάσω αὐτόν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 90",
        "book_usfm_code": "PSA",
        "chapter_number": 90,
        "verse_number": 16,
        "verse_text": "Μακρότητι ἡμερῶν ἐμπλήσω αὐτὸν, καὶ δείξω αὐτῷ τὸ σωτήριόν μου.",
        "iso-639-2_language_code": "grc"
    }
]