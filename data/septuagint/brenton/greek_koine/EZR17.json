    },
    {
        "book": "μετάφραση των εβδομήκοντα ΕΣΔΡΑΣ 17",
        "usfm_code": "EZR",
        "chapter": 17,
        "verse": "42",
        "text": "γἱοὶ Ἠρὰμ, χίλιοι δεκαεπτά.",
