    },
    {
        "book": "μετάφραση των εβδομήκοντα ΕΣΔΡΑΣ 17",
        "usfm_code": "EZR",
        "chapter": 17,
        "verse": "23",
        "text": "γἱοὶ Βεσεῒ, τριακόσιοι εἰκοσιτέσσαρες.",
