    },
    {
        "book": "μετάφραση των εβδομήκοντα ΕΣΔΡΑΣ 17",
        "usfm_code": "EZR",
        "chapter": 17,
        "verse": "55",
        "text": "υἱοὶ Βαρκουὲ, υἱοὶ Σισαρὰθ, υἱοὶ Θημὰ,",
        "iso-639-2_language_code": "grc"
