    },
    {
        "book": "μετάφραση των εβδομήκοντα ΕΣΔΡΑΣ 17",
        "usfm_code": "EZR",
        "chapter": 17,
        "verse": "25",
        "text": "γἱοὶ Γαβαὼν, ἐννενηκονταπέντε.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΕΣΔΡΑΣ 17",
        "usfm_code": "EZR",
        "chapter": 17,
        "verse": "26",
        "text": "γἱοὶ Βαιθαλὲμ, ἑκατὸν εἰκοσιτρεῖς· υἱοὶ Ἀτωφὰ, πεντηκονταέξ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΕΣΔΡΑΣ 17",
        "usfm_code": "EZR",
        "chapter": 17,
        "verse": "27",
        "text": "γἱοὶ Ἀναθὼθ, ἑκατὸν εἰκοσιοκτώ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΕΣΔΡΑΣ 17",
        "usfm_code": "EZR",
        "chapter": 17,
        "verse": "28",
        "text": "Ἄνδρες Βηθασμὼθ, τεσσαρακονταδύο.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΕΣΔΡΑΣ 17",
        "usfm_code": "EZR",
        "chapter": 17,
        "verse": "29",
        "text": "Ἄνδρες Καριαθαρὶμ, Καφιρὰ, καὶ Βηρὼθ, ἑπτακόσιοι τεσσαρακοντατρεῖς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΕΣΔΡΑΣ 17",
        "usfm_code": "EZR",
        "chapter": 17,
        "verse": "30",
        "text": "Ἄνδρες Ἀραμὰ, καὶ Γαβαὰ, ἑξακόσιοι εἴκοσι.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΕΣΔΡΑΣ 17",
        "usfm_code": "EZR",
        "chapter": 17,
        "verse": "31",
        "text": "Ἄνδρες Μαχεμὰς, ἑκατὸν εἰκοσιδύο.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΕΣΔΡΑΣ 17",
        "usfm_code": "EZR",
        "chapter": 17,
        "verse": "32",
        "text": "Ἄνδρες Βαιθὴλ καὶ Ἀῒ, ἑκατὸν εἰκοσιτρεῖς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΕΣΔΡΑΣ 17",
        "usfm_code": "EZR",
        "chapter": 17,
        "verse": "33",
        "text": "Ἄνδρες Ναβία, ἐκατὸν πεντηκονταδύο.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΕΣΔΡΑΣ 17",
        "usfm_code": "EZR",
        "chapter": 17,
        "verse": "34",
        "text": "Ἄνδρες Ἠλαμαὰρ, χίλιοι διακόσιοι πεντηκονταδύο.",
