    },
    {
        "book": "μετάφραση των εβδομήκοντα ΕΣΔΡΑΣ 17",
        "usfm_code": "EZR",
        "chapter": 17,
        "verse": "22",
        "text": "γἱοὶ Ἠσὰμ, τριακόσιοι εἰκοσιοκτώ.",
