    },
    {
        "book": "μετάφραση των εβδομήκοντα ΕΣΔΡΑΣ 17",
        "usfm_code": "EZR",
        "chapter": 17,
        "verse": "49",
        "text": "υἱοὶ Ἀνὰν, υἱοὶ Γαδὴλ, υἱοὶ Γαὰρ,",
