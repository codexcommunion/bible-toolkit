    },
    {
        "book": "μετάφραση των εβδομήκοντα ΕΣΔΡΑΣ 17",
        "usfm_code": "EZR",
        "chapter": 17,
        "verse": "35",
        "text": "γἱοὶ Ἠρὰμ, τριακόσιοι εἴκοσι.",
