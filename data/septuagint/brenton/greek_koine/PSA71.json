[
    {
        "book": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 71",
        "usfm_code": "PSA",
        "chapter": 71,
        "verse": "1",
        "text": "Εἰς Σαλωμών.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 71",
        "usfm_code": "PSA",
        "chapter": 71,
        "verse": "2",
        "text": "κρίνειν τὸν λαόν σου ἐν δικαιοσύνῃ, καὶ τοὺς πτωχούς σου ἐν κρίσει.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 71",
        "usfm_code": "PSA",
        "chapter": 71,
        "verse": "3",
        "text": "Ἀναλαβέτω τὰ ὄρη εἰρήνην τῷ λαῷ σου, καὶ οἱ βουνοί·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 71",
        "usfm_code": "PSA",
        "chapter": 71,
        "verse": "4",
        "text": "ἐν δικαιοσύνῃ κρινεῖ τοὺς πτωχοὺς τοῦ λαοῦ, καὶ σώσει τοὺς υἱοὺς τῶν πενήτων· καὶ ταπεινώσει συκοφάντην,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 71",
        "usfm_code": "PSA",
        "chapter": 71,
        "verse": "5",
        "text": "καὶ συμπαραμενεῖ τῷ ἡλίῳ, καὶ πρὸ τῆς σελήνης γενεὰς γενεῶν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 71",
        "usfm_code": "PSA",
        "chapter": 71,
        "verse": "6",
        "text": "Καταβήσεται ὡς ὑετὸς ἐπὶ πόκον, καὶ ὡσεὶ σταγόνες στάζουσαι ἐπὶ τὴν γῆν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 71",
        "usfm_code": "PSA",
        "chapter": 71,
        "verse": "7",
        "text": "Ἀνατελεῖ ἐν ταῖς ἡμέραις αὐτοῦ δικαιοσύνη, καὶ πλῆθος εἰρήνης ἕως οὗ ἀνταναιρεθῇ ἡ σελήνη.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 71",
        "usfm_code": "PSA",
        "chapter": 71,
        "verse": "8",
        "text": "Καὶ κατακυριεύσει ἀπὸ θαλάσσης ἕως θαλάσσης, καὶ ἀπὸ ποταμοῦ ἕως περάτων τῆς οἰκουμένης.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 71",
        "usfm_code": "PSA",
        "chapter": 71,
        "verse": "9",
        "text": "Ἐνώπιον αὐτοῦ προπεσοῦνται Αἰθίοπες, καὶ οἱ ἐχθροὶ αὐτοῦ χοῦν λείξουσι.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 71",
        "usfm_code": "PSA",
        "chapter": 71,
        "verse": "10",
        "text": "Βασιλεῖς Θαρσὶς καὶ αἱ νῆσοι δῶρα προσοίσουσι, βασιλεῖς Ἀράβων καὶ Σαβὰ δῶρα προσάξουσι.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 71",
        "usfm_code": "PSA",
        "chapter": 71,
        "verse": "11",
        "text": "Καὶ προσκυνήσουσιν αὐτῷ πάντες οἱ βασιλεῖς, πάντα τὰ ἔθνη δουλεύσουσιν αὐτῷ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 71",
        "usfm_code": "PSA",
        "chapter": 71,
        "verse": "12",
        "text": "Ὅτι ἐῤῥύσατο πτωχὸν ἐκ δυνάστου, καὶ πένητα ᾧ οὐχ ὑπῆρχε βοηθός.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 71",
        "usfm_code": "PSA",
        "chapter": 71,
        "verse": "13",
        "text": "Φείσεται πτωχοῦ καὶ πένητος, καὶ ψυχὰς πένητων σώσει.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 71",
        "usfm_code": "PSA",
        "chapter": 71,
        "verse": "14",
        "text": "Ἐκ τόκου καὶ ἐξ ἀδικίας λυτρώσεται τὰς ψυχὰς αὐτῶν, καὶ ἔντιμον τὸ ὄνομα αὐτῶν ἐνώπιον αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 71",
        "usfm_code": "PSA",
        "chapter": 71,
        "verse": "15",
        "text": "Καὶ ζήσεται, καὶ δοθήσεται αὐτῷ ἐκ τοῦ χρυσίου τῆς Ἀραβίας, καὶ προσεύξονται περὶ αὐτοῦ διαπαντός· ὅλην τὴν ἡμέραν εὐλογήσουσιν αὐτόν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 71",
        "usfm_code": "PSA",
        "chapter": 71,
        "verse": "16",
        "text": "Ἔσται στήριγμα ἐν τῇ γῇ ἐπʼ ἄκρων τῶν ὀρέων· ὑπεραρθήσεται ὑπὲρ τὸν Λίβανον ὁ καρπὸς αὐτοῦ, καὶ ἐξανθήσουσιν ἐκ πόλεως ὡσεὶ χόρτος τῆς γῆς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 71",
        "usfm_code": "PSA",
        "chapter": 71,
        "verse": "17",
        "text": "Ἔστω τὸ ὄνομα αὐτοῦ εὐλογημένον εἰς τοὺς αἰῶνας, πρὸ τοῦ ἡλίου διαμενεῖ τὸ ὄνομα αὐτοῦ, καὶ εὐλογηθήσονται ἐν αὐτῷ πᾶσαι αἱ φυλαὶ τῆς γῆς· πάντα τὰ ἔθνη μακαριοῦσιν αὐτόν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 71",
        "usfm_code": "PSA",
        "chapter": 71,
        "verse": "18",
        "text": "Εὐλογητὸς Κύριος ὁ Θεὸς τοῦ Ἰσραὴλ, ὁ ποιῶν θαυμάσια μόνος,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 71",
        "usfm_code": "PSA",
        "chapter": 71,
        "verse": "19",
        "text": "καὶ εὐλογητὸν τὸ ὄνομα τῆς δόξης αὐτοῦ εἰς τὸν αἰῶνα καὶ εἰς αἰῶνα τοῦ αἰῶνος· καὶ πληρωθήσεται τῆς δόξης αὐτοῦ πᾶσα ἡ γῆ. γένοιτο, γένοιτο.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 71",
        "usfm_code": "PSA",
        "chapter": 71,
        "verse": "20",
        "text": "Ἐξέλιπον οἱ ὕμνοι Δαυὶδ τοῦ υἱοῦ Ἰεσσαί.",
        "iso-639-2_language_code": "grc"
    }
]