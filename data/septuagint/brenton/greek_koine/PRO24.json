[
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "1",
        "text": "Υἱέ, μὴ ζηλώσῃς κακοὺς ἄνδρας, μηδὲ ἐπιθυμήσῃς εἶναι μετʼ αὐτῶν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "2",
        "text": "Ψευδῆ γὰρ μελετᾷ ἡ καρδία αὐτῶν, καὶ πόνους τὰ χείλη αὐτῶν λαλεῖ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "3",
        "text": "Μετὰ σοφίας οἰκοδομεῖται οἶκος, καὶ μετὰ συνέσεως ἀνορθοῦται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "4",
        "text": "Μετὰ αἰσθήσεως ἐμπίμπλανται ταμιεῖα ἐκ παντὸς πλούτου τιμίου καὶ καλοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "5",
        "text": "Κρείσσων σοφὸς ἰσχυροῦ, καὶ ἀνὴρ φρόνησιν ἔχων γεωργίου μεγάλου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "6",
        "text": "Μετὰ κυβερνήσεως γίνεται πόλεμος, βοήθεια δὲ μετὰ καρδίας βουλευτικῆς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "7",
        "text": "Σοφία καὶ ἔννοια ἀγαθὴ ἐν πύλαις σοφῶν· σοφοὶ οὐκ ἐκκλίνουσιν ἐκ στόματος Κυρίου,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "8",
        "text": "ἀλλὰ λογίζονται ἐν συνεδρίοις· ἀπαιδεύτοις συναντᾷ θάνατος,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "9",
        "text": "ἀποθνήσκει δὲ ἄφρων ἐν ἁμαρτίαις· ἀκαθαρσία δὲ ἀνδρὶ λοιμῷ,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "10",
        "text": "ἐμμολυνθήσεται ἐν ἡμέρᾳ κακῇ, καὶ ἐν ἡμέρᾳ θλίψεως ἕως ἂν ἐκλίπῃ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "11",
        "text": "Ῥῦσαι ἀγομένους εἰς θάνατον, καὶ ἐκπρίου κτεινομένους, μὴ φείσῃ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "12",
        "text": "Ἐὰν δὲ εἴπῃς, οὐκ οἶδα τοῦτον, γίνωσκε, ὅτι Κύριος καρδίας πάντων γινώσκει· καὶ ὁ πλάσας πνοὴν πᾶσιν, αὐτὸς οἶδε πάντα, ὃς ἀποδίδωσιν ἑκάστῳ κατὰ τὰ ἔργα αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "13",
        "text": "Φάγε μέλι υἱὲ, ἀγαθὸν γὰρ κηρίον, ἵνα γλυκανθῇ σου ὁ φάρυγξ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "14",
        "text": "Οὕτως αἰσθητήσῃ σοφίαν τῇ σῇ ψυχῇ· ἐὰν γὰρ εὕρῃς, ἔσται καλὴ ἡ τελευτή σου, καὶ ἐλπίς σε οὐκ ἐγκαταλείψει.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "15",
        "text": "Μὴ προσαγάγῃς ἀσεβῆ νομῇ δικαίων, μηδὲ ἀπατηθῇς χορτασίᾳ κοιλίας.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "16",
        "text": "Ἑπτάκις γὰρ πεσεῖται δίκαιος καὶ ἀναστήσεται, οἱ δὲ ἀσεβεῖς ἀσθενήσουσιν ἐν κακοῖς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "17",
        "text": "Ἐὰν πέσῃ ὁ ἐχθρός σου, μὴ ἐπιχαρῇς αὐτῷ, ἐν δὲ τῷ ὑποσκελίσματι αὐτοῦ μὴ ἐπαίρου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "18",
        "text": "Ὅτι ὄψεται Κύριος καὶ οὐκ ἀρέσει αὐτῷ, καὶ ἀποστρέψει τὸν θυμὸν αὐτοῦ ἀπʼ αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "19",
        "text": "Μὴ χαῖρε ἐπὶ κακοποιοῖς, μηδὲ ζήλου ἁμαρτωλούς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "20",
        "text": "Οὐ γὰρ μὴ γένηται ἔκγονα πονηρῷ, λαμπτὴρ δὲ ἀσεβῶν σβεσθήσεται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "21",
        "text": "Φοβοῦ τὸν Θεὸν υἱὲ, καὶ βασιλέα, καὶ μηθʼ ἑτέρῳ αὐτῶν ἀπειθήσῃς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "22",
        "text": "Ἐξαίφνης γὰρ τίσονται τοὺς ἀσεβεῖς, τὰς δὲ τιμωρίας ἀμφοτέρων τίς γνώσεται;",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "22a",
        "text": "Λόγον φυλασσόμενος υἱὸς ἀπωλείας ἐκτὸς ἔσται, [δεχόμενος δὲ ἐδέξατο αὐτόν·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "22b",
        "text": "μηδὲν ψεῦδος ἀπὸ γλώσσης βασιλεῖ λεγέσθω, καὶ οὐδὲν ψεῦδος ἀπὸ γλώσσης αὐτοῦ οὐ μή ἐξέλθῃ·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "22c",
        "text": "μάχαιρα γλῶσσα βασιλέως καὶ οὐ σαρκίνη, ὃς δʼ ἂν παραδοθῇ συντριβήσεται·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "22d",
        "text": "ἐὰν γὰρ ὀξυνθῇ ὁ θυμὸς αὐτοῦ, σὺν νεύροις ἀνθρώπους ἀναλίσκει,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "22e",
        "text": "καὶ ὀστᾶ ἀνθρώπων κατατρώγει, καὶ συγκαίει ὥσπερ φλὸξ, ὥστε ἄβρωτα εἶναι νεοσσοῖς ἀετῶν·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "22f",
        "text": "τοὺς ἐμοὺς λόγους υἱὲ φοβήθητι, καὶ δεξάμενος αὐτοὺς μετανόει.]",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "22g",
        "text": "Ἀφρονέστατος γάρ εἰμι ἁπάντων ἀνθρώπων, καὶ φρόνησις ἀνθρώπων οὐκ ἔστιν ἐν ἐμοί.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "22h",
        "text": "Θεὸς δεδίδαχέ με σοφίαν, καὶ γνῶσιν ἁγίων ἔγνωκα.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "22i",
        "text": "Τίς ἀνέβη εἰς τὸν οὐρανὸν καὶ κατέβη; τίς συνήγαγεν ἀνέμους ἐν κόλπῳ; τίς συνέστρεψεν ὕδωρ ἐν ἱματίῳ; τίς ἐκράτησε πάντων τῶν ἄκρων τῆς γῆς; τί ὄνομα αὐτῷ; ἢ τί ὄνομα τοῖς τέκνοις αὐτοῦ;",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "22k",
        "text": "Πάντες γὰρ λόγοι Θεοῦ πεπυρωμένοι, ὑπερασπίζει δὲ αὐτὸς τῶν εὐλαβουμένων αὐτόν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "22l",
        "text": "Μὴ προσθῇς τοῖς λόγοις αὐτοῦ, ἵνα μὴ ἐλέγξῃ σε, καὶ ψευδὴς γένῃ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "22m",
        "text": "Δύο αἰτοῦμαι παρὰ σοῦ, μὴ ἀφέλῃς μου χάριν πρὸ τοῦ ἀποθανεῖν με.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "22n",
        "text": "Μάταιον λόγον καὶ ψευδῆ μακράν μου ποίησον, πλοῦτον δὲ καὶ πενίαν μή μοι δῷς, σύνταξον δέ μοι τὰ δέοντα καὶ τὰ αὐτάρκη·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "22o",
        "text": "Ἵνα μὴ πλησθεὶς ψευδὴς γένωμαι, καὶ εἴπω, τίς με ὁρᾷ; ἢ πενηθεὶς κλέψω, καὶ ὀμόσω τὸ ὄνομα τοῦ Θεοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "22p",
        "text": "Μὴ παραδῷς οἰκέτην εἰς χεῖρας δεσπότου, μήποτε καταράσηταί σε καὶ ἀφανισθῇς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "22q",
        "text": "Ἔκγονον κακὸν πατέρα καταρᾶται, τὴς δὲ μητέρα οὐκ εὐλογεῖ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "22r",
        "text": "Ἔκγονον κακὸν δίκαιον ἑαυτὸν κρίνει, τὴν δʼ ἔξοδον αὐτοῦ οὐκ ἀπένιψεν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "22s",
        "text": "Ἔκγονον κακὸν ὑψηλοὺς ὀφθαλμοὺς ἔχει, τοῖς δὲ βλεφάροις αὐτοῦ ἐπαίρεται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "22t",
        "text": "Ἔκγονον κακὸν μαχαίρας τοὺς ὀδόντας ἔχει, καὶ τὰς μύλας, τομίδας, ὥστε ἀναλίσκειν καὶ κατεσθίειν τοὺς ταπεινοὺς ἀπὸ τῆς γῆς, καὶ τοὺς πένητας αὐτῶν ἐξ ἀνθρώπων.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "23",
        "text": "Ταῦτα δὲ λέγω ὑμῖν τοῖς σοφοῖς ἐπιγινώσκειν· αἰδεῖσθαι πρόσωπον ἐν κρίσει οὐ καλόν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "24",
        "text": "Ὁ εἰπὼν τὸν ἀσεβῆ, δίκαιός ἐστιν, ἐπικατάρατος λαοῖς ἔσται καὶ μισητὸς εἰς ἔθνη.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "25",
        "text": "Οἱ δὲ ἐλέγχοντες βελτίους φανοῦνται, ἐπʼ αὐτοὺς δὲ ἥξει εὐλογία·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "26",
        "text": "χείλη δὲ φιλήσουσιν ἀποκρινόμενα λόγους ἀγαθούς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "27",
        "text": "Ἑτοίμαζε εἰς τὴν ἔξοδον τὰ ἔργα σου, καὶ παρασκευάζου εἰς τὸν ἀγρὸν, καὶ πορεύου κατόπισθέν μου, καὶ ἀνοικοδομήσεις τὸν οἶκόν σου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "28",
        "text": "Μὴ ἴσθι ψευδὴς μάρτυς ἐπὶ σὸν πολίτην, μηδὲ πλατύνου σοῖς χείλεσι.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "29",
        "text": "Μὴ εἴπῃς, ὃν τρόπον ἐχρήσατό μοι, χρήσομαι αὐτῷ, τίσομαι δὲ αὐτὸν ἅ με ἠδίκησεν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "30",
        "text": "Ὥσπερ γεώργιον ἀνὴρ ἄφρων, καὶ ὥσπερ ἀμπελὼν ἄνθρωπος ἐνδεὴς φρενῶν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "31",
        "text": "Ἐὰν ἀφῇς αὐτὸν, χερσωθήσεται καὶ χορτομανήσει ὅλος, καὶ γίνεται ἐκλελειμμένος, οἱ δὲ φραγμοὶ τῶν λίθων αὐτοῦ κατασκάπτονται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "32",
        "text": "Ὕστερον ἐγὼ μετενόησα, ἐπέβλεψα τοῦ ἐκλέξασθαι παιδείαν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "33",
        "text": "Ὀλίγον νυστάζω, ὀλίγον δὲ καθυπνῶ, ὀλίγον δὲ ἐναγκαλίζομαι χερσὶ στήθη.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "34",
        "text": "Ἐὰν δὲ τοῦτο ποιῇς, ἥξει προπορευομένη ἡ πενία σου, καὶ ἡ ἔνδειά σου ὥσπερ ἀγαθὸς δρομεύς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "35",
        "text": "Τῇ βδέλλῃ τρεῖς θυγατέρες ἦσαν ἀγαπήσει ἀγαπώμεναι, καὶ αἱ τρεῖς αὗται οὐκ ἐνεπίμπλασαν αὐτὴν, καὶ ἡ τετάρτη οὐκ ἠρκέσθη εἰπεῖν, ἱκανόν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "36",
        "text": "Ἄδης καὶ ἔρως γυναικὸς, καὶ γῆ οὐκ ἐμπιπλαμένη ὕδατος, καὶ ὕδωρ καὶ πῦρ οὐ μὴ εἴπωσιν, ἀρκεῖ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "37",
        "text": "Ὀφθαλμὸν καταγελῶντα πατρὸς, καὶ ἀτιμάζοντα γῆρας μητρὸς, ἐκκόψαισαν αὐτὸν κόρακες ἐκ τῶν φαράγγων, καὶ καταφάγοισαν αὐτὸν νεοσσοὶ ἀετῶν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "38",
        "text": "Τρία δέ ἐστιν ἀδύνατά μοι νοῆσαι, καὶ τὸ τέταρτον οὐκ ἐπιγινώσκω·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "39",
        "text": "Ἴχνη ἀετοῦ πετομένου, καὶ ὁδοὺς ὄφεως ἐπὶ πέτρας, καὶ τρίβους νηὸς ποντοπορούσης, καὶ ὁδοὺς ἀνδρὸς ἐν νεότητι.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "40",
        "text": "Τοιαύτη ὁδὸς γυναικὸς μοιχαλίδος, ἣ ὅτʼ ἂν πράξῃ ἀπονιψαμένη, οὐδέν φησι πεπραχέναι ἄτοπον.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "41",
        "text": "Διὰ τριῶν σείεται ἡ γῆ, τὸ δὲ τέταρτον οὐ δύναται φέρειν·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "42",
        "text": "Ἐὰν οἰκέτης βασιλεύσῃ, καὶ ἄφρων πλησθῇ σιτίων,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "43",
        "text": "καὶ οἰκέτις ἐὰν ἐκβάλῃ τὴν ἑαυτῆς κυρίαν, καὶ μισητὴ γυνὴ ἐὰν τύχῃ ἀνδρὸς ἀγαθοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "44",
        "text": "Τέσσαρα δὲ ἐλάχιστα ἐπὶ τῆς γῆς, ταῦτα δέ ἐστι σοφώτερα τῶν σοφῶν·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "45",
        "text": "Οἱ μύρμηκες οἷς μή ἐστιν ἰσχὺς, καὶ ἑτοιμάζονται θέρους τὴν τροφήν·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "46",
        "text": "Καὶ οἱ χοιρογρύλλιοι ἔθνος οὐκ ἰσχυρὸν, οἳ ἐποιήσαντο ἐν πέτραις τοὺς ἑαυτῶν οἴκους·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "47",
        "text": "Ἀβασίλευτόν ἐστιν ἡ ἀκρὶς, καὶ στρατεύει ἀφʼ ἑνὸς κελεύσματος εὐτάκτως·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "48",
        "text": "Καὶ καλαβώτης χερσὶν ἐρειδόμενος, καὶ εὐάλωτος ὢν, κατοικεῖ ἐν ὀχυρώμασι βασιλέων.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "49",
        "text": "Τρία δέ ἐστιν ἃ εὐόδως πορεύεται, καὶ τέταρτον ὃ καλῶς διαβαίνει·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "50",
        "text": "Σκύμνος λέοντος ἰσχυρότερος κτηνῶν, ὃς οὐκ ἀποστρέφεται, οὐδὲ καταπτήσσει κτῆνος·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "51",
        "text": "Καὶ ἀλέκτωρ ἐμπεριπατῶν θηλείαις εὔψυχος, καὶ τράγος ἡγούμενος αἰπολίου, καὶ βασιλεὺς δημηγορῶν ἐν ἔθνει.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "52",
        "text": "Ἐὰν πρόῃ σεαυτὸν ἐν εὐφροσύνῃ, καὶ ἐκτείνῃς τὴν χεῖρά σου μετὰ μάχης, ἀτιμασθήσῃ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "53",
        "text": "Ἄμελγε γάλα, καὶ ἔσται βούτυρον· ἐὰν δὲ ἐκπιέζῃς μυκτῆρας ἐξελεύσεται αἷμα, ἐὰν δὲ ἐξέλκῃς λόγους, ἐξελεύσονται κρίσεις καὶ μάχαι.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "54",
        "text": "Οἱ ἐμοὶ λόγοι εἴρηνται ὑπὸ Θεοῦ, βασιλέως χρηματισμὸς, ὃν ἐπαίδευσεν ἡ μήτηρ αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "55",
        "text": "Τί τέκνον τηρήσεις; τί; ῥήσεις Θεοῦ· πρωτογενὲς σοὶ λέγω υἱέ· τί τέκνον ἐμῆς κοιλίας; τί τέκνον ἐμῶν εὐχῶν;",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "56",
        "text": "Μὴ δῷς γυναιξὶ σὸν πλοῦτον, καὶ τὸν σὸν νοῦν καὶ βίον εἰς ὑστεροβουλίαν·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "57",
        "text": "μετὰ βουλῆς πάντα ποίει, μετὰ βουλῆς οἰνοπότει. Οἱ δυνάσται θυμώδεις εἰσὶν, οἶνον δὲ μὴ πινέτωσαν,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "58",
        "text": "ἵνα μὴ πιόντες ἐπιλάθωνται τῆς σοφίας, καὶ ὀρθὰ κρῖναι οὐ μὴ δύνωνται τοὺς ἀσθενεῖς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "59",
        "text": "Δίδοτε μέθην τοῖς ἐν λύπαις, καὶ οἶνον πίνειν τοῖς ἐν ὀδύναις,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "60",
        "text": "ἵνα ἐπιλάθωνται τῆς πενίας, καὶ τῶν πόνων μὴ μνησθῶσιν ἔτι.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "61",
        "text": "Ἄνοιγε σὸν στόμα λόγῳ Θεοῦ, καὶ κρίνε πάντας ὑγιῶς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 24",
        "usfm_code": "PRO",
        "chapter": 24,
        "verse": "62",
        "text": "Ἄνοιγε σὸν στόμα καὶ κρίνε δικαίως, διάκρινε δὲ πένητα καὶ ἀσθενῆ.",
        "iso-639-2_language_code": "grc"
    }
]