[
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 111",
        "book_usfm_code": "PSA",
        "chapter_number": 111,
        "verse_number": 1,
        "verse_text": "Ἀλληλούϊα.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 111",
        "book_usfm_code": "PSA",
        "chapter_number": 111,
        "verse_number": 2,
        "verse_text": "Δυνατὸν ἐν τῇ γῇ ἔσται τὸ σπέρμα αὐτοῦ, γενεὰ εὐθέων εὐλογηθήσεται·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 111",
        "book_usfm_code": "PSA",
        "chapter_number": 111,
        "verse_number": 3,
        "verse_text": "Δόξα καὶ πλοῦτος ἐν τῷ οἴκῳ αὐτοῦ, καὶ ἡ δικαιοσύνη αὐτοῦ μένει εἰς τὸν αἰῶνα τοῦ αἰῶνος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 111",
        "book_usfm_code": "PSA",
        "chapter_number": 111,
        "verse_number": 4,
        "verse_text": "Ἐξανέτειλεν ἐν σκότει φῶς τοῖς εὐθέσιν· ἐλεήμων καὶ οἰκτίρμων καὶ δίκαιος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 111",
        "book_usfm_code": "PSA",
        "chapter_number": 111,
        "verse_number": 5,
        "verse_text": "Χρηστὸς ἀνὴρ ὁ οἰκτείρων καὶ κιχρῶν, οἰκονομήσει τοὺς λόγους αὐτοῦ ἐν κρίσει,",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 111",
        "book_usfm_code": "PSA",
        "chapter_number": 111,
        "verse_number": 6,
        "verse_text": "ὅτι εἰς τὸν αἰῶνα οὐ σαλευθήσεται· εἰς μνημόσυνον αἰώνιον ἔσται δίκαιος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 111",
        "book_usfm_code": "PSA",
        "chapter_number": 111,
        "verse_number": 7,
        "verse_text": "Ἀπὸ ἀκοῆς πονηρᾶς οὐ φοβηθήσεται· ἑτοίμη ἡ καρδία αὐτοῦ ἐλπίζειν ἐπὶ Κύριον·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 111",
        "book_usfm_code": "PSA",
        "chapter_number": 111,
        "verse_number": 8,
        "verse_text": "Ἐστήρικται ἡ καρδία αὐτοῦ, οὐ φοβηθῇ, ἕως οὗ ἐπίδῃ ἐπὶ τοὺς ἐχθροὺς αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 111",
        "book_usfm_code": "PSA",
        "chapter_number": 111,
        "verse_number": 9,
        "verse_text": "Ἐσκόρπισεν, ἔδωκε τοῖς πένησιν, ἡ δικαιοσύνη αὐτοῦ μένει εἰς τὸν αἰῶνα τοῦ αἰῶνος· τὸ κέρας αὐτοῦ ὑψωθήσεται ἐν δόξῃ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΨΑΛΜΟΙ 111",
        "book_usfm_code": "PSA",
        "chapter_number": 111,
        "verse_number": 10,
        "verse_text": "Ἁμαρτωλὸς ὄψεται καὶ ὀργισθήσεται, τοὺς ὀδόντας αὐτοῦ βρύξει καὶ τακήσεται· ἐπιθυμία ἁμαρτωλοῦ ἀπολεῖται.",
        "iso-639-2_language_code": "grc"
    }
]