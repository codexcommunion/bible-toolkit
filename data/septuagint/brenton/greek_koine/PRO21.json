[
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 1,
        "verse_text": "Ὥσπερ ὁρμὴ ὕδατος, οὕτως καρδία βασιλέως ἐν χειρὶ Θεοῦ, οὗ ἐὰν θέλων νεῦσαι ἐκεῖ ἔκλινεν αὐτήν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 2,
        "verse_text": "Πᾶς ἀνὴρ φαίνεται ἑαυτῷ δίκαιος, κατευθύνει δὲ καρδίας Κύριος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 3,
        "verse_text": "Ποιεῖν δίκαια καὶ ἀληθεύειν, ἀρεστὰ παρὰ Θεῷ μᾶλλον ἢ θυσιῶν αἷμα.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 4,
        "verse_text": "Μεγαλόφρων ἐν ὕβρει θρασυκάρδιος, λαμπτὴρ δὲ ἀσεβῶν ἁμαρτία.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 6,
        "verse_text": "Ὁ ἐνεργῶν θησαυρίσματα γλώσσῃ ψευδεῖ, μάταια διώκει ἐπὶ παγίδας θανάτου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 7,
        "verse_text": "Ὄλεθρος ἀσεβέσιν ἐπιξενωθήσεται, οὐ γὰρ βούλονται πράσσειν τὰ δίκαια.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 8,
        "verse_text": "Πρὸς τοὺς σκολιοὺς σκολιὰς ὁδοὺς ἀποστέλλει ὁ Θεὸς, ἁγνὰ γὰρ καὶ ὀρθὰ τὰ ἔργα αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 9,
        "verse_text": "Κρεῖσσον οἰκεῖν ἐπὶ γωνίας ὑπαίθρου, ἢ ἐν κεκονιαμένοις μετὰ ἀδικίας καὶ ἐν οἴκῳ κοινῷ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 10,
        "verse_text": "Ψυχὴ ἀσεβοῦς οὐκ ἐλεηθήσεται ὑπʼ οὐδενὸς τῶν ἀνθρώπων.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 11,
        "verse_text": "Ζημιουμένου ἀκολάστου πανουργότερος γίνεται ὁ ἄκακος, συνιῶν δὲ σοφὸς δέξεται γνῶσιν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 12,
        "verse_text": "Συνιεῖ δίκαιος καρδίας ἀσεβῶν, καὶ φαυλίζει ἀσεβεῖς ἐν κακοῖς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 13,
        "verse_text": "Ὃς φράσσει τὰ ὦτα αὐτοῦ τοῦ μὴ ἐπακοῦσαι ἀσθενοῦς, καὶ αὐτὸς ἐπικαλέσεται καὶ οὐκ ἔσται ὁ εἰσακούων.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 14,
        "verse_text": "Δόσις λάθριος ἀνατρέπει ὀργάς, δώρων δὲ ὁ φειδόμενος θυμὸν ἐγείρει ἰσχυρόν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 15,
        "verse_text": "Εὐφροσύνη δικαίων ποιεῖν κρίμα, ὅσιος δὲ ἀκάθαρτος παρὰ κακούργοις.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 16,
        "verse_text": "Ἀνὴρ πλανώμενος ἐξ ὁδοῦ δικαιοσύνης, ἐν συναγωγῇ γιγάντων ἀναπαύσεται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 17,
        "verse_text": "Ἀνὴρ ἐνδεὴς ἀγαπᾷ εὐφροσύνην, φιλῶν οἶνον καὶ ἔλαιον εἰς πλοῦτον·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 18,
        "verse_text": "Περικάθαρμα δὲ δικαίου ἄνομος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 19,
        "verse_text": "Κρεῖσσον οἰκεῖν ἐν τῇ ἐρήμῳ, ἢ μετὰ γυναικὸς μαχίμου καὶ γλωσσώδους καὶ καὶ ὀργίλου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 20,
        "verse_text": "Θησαυρὸς ἐπιθυμητὸς ἀναπαύσεται ἐπὶ στόματος σοφοῦ, ἄφρονες δὲ ἄνδρες καταπίονται αὐτόν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 21,
        "verse_text": "Ὁδὸς δικαιοσύνης καὶ ἐλεημοσύνης εὑρήσει ζωὴν καὶ δόξαν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 22,
        "verse_text": "Πόλεις ὀχυρὰς ἐπέβη σοφὸς, καὶ καθεῖλε τὸ ὀχύρωμα ἐφʼ ᾧ ἐπεποίθεισαν οἱ ἀσεβεῖς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 23,
        "verse_text": "Ὃς φυλάσσει τὸ στόμα αὐτοῦ καὶ τὴν γλῶσσαν, διατηρεῖ ἐκ θλίψεως τὴν ψυχὴν αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 24,
        "verse_text": "Θρασὺς καὶ αὐθάδης καὶ ἀλαζὼν λοιμὸς καλεῖται, ὃς δὲ μνησικακεῖ παράνομος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 25,
        "verse_text": "Ἐπιθυμίαι ὀκνηρὸν ἀποκτείνουσιν, οὐ γὰρ προαιροῦνται αἱ χεῖρες αὐτοῦ ποιεῖν τι.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 26,
        "verse_text": "Ἀσεβὴς ἐπιθυμεῖ ὅλην τὴν ἡμέραν ἐπιθυμίας κακὰς, ὁ δὲ δίκαιος ἐλεᾷ καὶ οἰκτείρει ἀφειδῶς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 27,
        "verse_text": "Θυσίαι ἀσεβῶν βδέλυγμα Κυρίῳ, καὶ γὰρ παρανόμως προσφέρουσιν αὐτάς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 28,
        "verse_text": "Μάρτυς ψευδὴς ἀπολεῖται, ἀνὴρ δὲ ὑπήκοος φυλασσόμενος λαλήσει.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 29,
        "verse_text": "Ἀσεβὴς ἀνὴρ ἀναιδῶς ὑφίσταται προσώπῳ, ὁ δὲ εὐθὺς αὐτὸς συνιεῖ τὰς ὁδοὺς αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 30,
        "verse_text": "Οὐκ ἔστι σοφία, οὐκ ἔστιν ἀνδρεία, οὐκ ἔστι βουλὴ πρὸς τὸν ἀσεβῆ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "book_usfm_code": "PRO",
        "chapter_number": 21,
        "verse_number": 31,
        "verse_text": "Ἵππος ἑτοιμάζεται εἰς ἡμέραν πολέμου, παρὰ δὲ Κυρίου ἡ βοήθεια.",
        "iso-639-2_language_code": "grc"
    }
]