[
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "1",
        "text": "Ὥσπερ ὁρμὴ ὕδατος, οὕτως καρδία βασιλέως ἐν χειρὶ Θεοῦ, οὗ ἐὰν θέλων νεῦσαι ἐκεῖ ἔκλινεν αὐτήν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "2",
        "text": "Πᾶς ἀνὴρ φαίνεται ἑαυτῷ δίκαιος, κατευθύνει δὲ καρδίας Κύριος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "3",
        "text": "Ποιεῖν δίκαια καὶ ἀληθεύειν, ἀρεστὰ παρὰ Θεῷ μᾶλλον ἢ θυσιῶν αἷμα.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "4",
        "text": "Μεγαλόφρων ἐν ὕβρει θρασυκάρδιος, λαμπτὴρ δὲ ἀσεβῶν ἁμαρτία.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "6",
        "text": "Ὁ ἐνεργῶν θησαυρίσματα γλώσσῃ ψευδεῖ, μάταια διώκει ἐπὶ παγίδας θανάτου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "7",
        "text": "Ὄλεθρος ἀσεβέσιν ἐπιξενωθήσεται, οὐ γὰρ βούλονται πράσσειν τὰ δίκαια.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "8",
        "text": "Πρὸς τοὺς σκολιοὺς σκολιὰς ὁδοὺς ἀποστέλλει ὁ Θεὸς, ἁγνὰ γὰρ καὶ ὀρθὰ τὰ ἔργα αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "9",
        "text": "Κρεῖσσον οἰκεῖν ἐπὶ γωνίας ὑπαίθρου, ἢ ἐν κεκονιαμένοις μετὰ ἀδικίας καὶ ἐν οἴκῳ κοινῷ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "10",
        "text": "Ψυχὴ ἀσεβοῦς οὐκ ἐλεηθήσεται ὑπʼ οὐδενὸς τῶν ἀνθρώπων.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "11",
        "text": "Ζημιουμένου ἀκολάστου πανουργότερος γίνεται ὁ ἄκακος, συνιῶν δὲ σοφὸς δέξεται γνῶσιν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "12",
        "text": "Συνιεῖ δίκαιος καρδίας ἀσεβῶν, καὶ φαυλίζει ἀσεβεῖς ἐν κακοῖς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "13",
        "text": "Ὃς φράσσει τὰ ὦτα αὐτοῦ τοῦ μὴ ἐπακοῦσαι ἀσθενοῦς, καὶ αὐτὸς ἐπικαλέσεται καὶ οὐκ ἔσται ὁ εἰσακούων.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "14",
        "text": "Δόσις λάθριος ἀνατρέπει ὀργάς, δώρων δὲ ὁ φειδόμενος θυμὸν ἐγείρει ἰσχυρόν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "15",
        "text": "Εὐφροσύνη δικαίων ποιεῖν κρίμα, ὅσιος δὲ ἀκάθαρτος παρὰ κακούργοις.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "16",
        "text": "Ἀνὴρ πλανώμενος ἐξ ὁδοῦ δικαιοσύνης, ἐν συναγωγῇ γιγάντων ἀναπαύσεται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "17",
        "text": "Ἀνὴρ ἐνδεὴς ἀγαπᾷ εὐφροσύνην, φιλῶν οἶνον καὶ ἔλαιον εἰς πλοῦτον·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "18",
        "text": "Περικάθαρμα δὲ δικαίου ἄνομος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "19",
        "text": "Κρεῖσσον οἰκεῖν ἐν τῇ ἐρήμῳ, ἢ μετὰ γυναικὸς μαχίμου καὶ γλωσσώδους καὶ καὶ ὀργίλου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "20",
        "text": "Θησαυρὸς ἐπιθυμητὸς ἀναπαύσεται ἐπὶ στόματος σοφοῦ, ἄφρονες δὲ ἄνδρες καταπίονται αὐτόν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "21",
        "text": "Ὁδὸς δικαιοσύνης καὶ ἐλεημοσύνης εὑρήσει ζωὴν καὶ δόξαν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "22",
        "text": "Πόλεις ὀχυρὰς ἐπέβη σοφὸς, καὶ καθεῖλε τὸ ὀχύρωμα ἐφʼ ᾧ ἐπεποίθεισαν οἱ ἀσεβεῖς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "23",
        "text": "Ὃς φυλάσσει τὸ στόμα αὐτοῦ καὶ τὴν γλῶσσαν, διατηρεῖ ἐκ θλίψεως τὴν ψυχὴν αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "24",
        "text": "Θρασὺς καὶ αὐθάδης καὶ ἀλαζὼν λοιμὸς καλεῖται, ὃς δὲ μνησικακεῖ παράνομος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "25",
        "text": "Ἐπιθυμίαι ὀκνηρὸν ἀποκτείνουσιν, οὐ γὰρ προαιροῦνται αἱ χεῖρες αὐτοῦ ποιεῖν τι.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "26",
        "text": "Ἀσεβὴς ἐπιθυμεῖ ὅλην τὴν ἡμέραν ἐπιθυμίας κακὰς, ὁ δὲ δίκαιος ἐλεᾷ καὶ οἰκτείρει ἀφειδῶς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "27",
        "text": "Θυσίαι ἀσεβῶν βδέλυγμα Κυρίῳ, καὶ γὰρ παρανόμως προσφέρουσιν αὐτάς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "28",
        "text": "Μάρτυς ψευδὴς ἀπολεῖται, ἀνὴρ δὲ ὑπήκοος φυλασσόμενος λαλήσει.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "29",
        "text": "Ἀσεβὴς ἀνὴρ ἀναιδῶς ὑφίσταται προσώπῳ, ὁ δὲ εὐθὺς αὐτὸς συνιεῖ τὰς ὁδοὺς αὐτοῦ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "30",
        "text": "Οὐκ ἔστι σοφία, οὐκ ἔστιν ἀνδρεία, οὐκ ἔστι βουλὴ πρὸς τὸν ἀσεβῆ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 21",
        "usfm_code": "PRO",
        "chapter": 21,
        "verse": "31",
        "text": "Ἵππος ἑτοιμάζεται εἰς ἡμέραν πολέμου, παρὰ δὲ Κυρίου ἡ βοήθεια.",
        "iso-639-2_language_code": "grc"
    }
]