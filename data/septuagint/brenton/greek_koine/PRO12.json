[
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 1,
        "verse_text": "Ὁ ἀγαπῶν παιδείαν, ἀγαπᾷ αἴσθησιν· ὁ δὲ μισῶν ἐλέγχους, ἄφρων.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 2,
        "verse_text": "Κρείσσων ὁ εὑρὼν χάριν παρὰ Κυρίῳ, ἀνὴρ δὲ παράνομος παρασιωπηθήσεται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 3,
        "verse_text": "Οὐ κατορθώσει ἄνθρωπος ἐξ ἀνόμου, αἱ δὲ ῥίζαι τῶν δικαίων οὐκ ἐξαρθήσονται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 4,
        "verse_text": "Γυνὴ ἀνδρεία στέφανος τῷ ἀνδρὶ αὐτῆς· ὥσπερ δὲ ἐν ξύλῳ σκώληξ, οὕτως ἄνδρα ἀπόλλυσι γυνὴ κακοποιός.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 5,
        "verse_text": "Λογισμοὶ δικαίων κρίματα, κυβερνῶσι δὲ ἀσεβεῖς δόλους.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 6,
        "verse_text": "Λόγοι ἀσεβῶν δόλιοι, στόμα δὲ ὀρθῶν ῥύσεται αὐτούς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 7,
        "verse_text": "Οὗ ἐὰν στραφῇ ὁ ἀσεβὴς, ἀφανίζεται, οἶκοι δὲ δικαίων παραμένουσι·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 8,
        "verse_text": "Στόμα συνετοῦ ἐγκωμιάζεται ὑπὸ ἀνδρὸς, νωθροκάρδιος δὲ μυκτηρίζεται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 9,
        "verse_text": "Κρείσσων ἀνὴρ ἐν ἀτιμίᾳ δουλεύων ἑαυτῷ, ἢ τιμὴν ἑαυτῷ περιτιθεὶς καὶ προσδεόμενος ἄρτου.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 10,
        "verse_text": "Δίκαιος οἰκτείρει ψυχὰς κτηνῶν αὐτοῦ, τὰ δὲ σπλάγχνα τῶν ἀσεβῶν ἀνελεήμονα.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 11,
        "verse_text": "Ὁ ἐργαζόμενος τὴν ἑαυτοῦ γῆν, ἐμπλησθήσεται ἄρτων, οἱ δὲ διώκοντες μάταια, ἐνδεεῖς φρενῶν·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 12,
        "verse_text": "ὅς ἐστιν ἡδὺς ἐν οἴνων διατριβαῖς, ἐν τοῖς ἑαυτοῦ ὀχυρώμασι καταλείψει ἀτιμίαν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 12,
        "verse_text": "Ἐπιθυμίαι ἀσεβῶν κακαὶ, αἱ δὲ ῥίζαι τῶν εὐσεβῶν ἐν ὀχυρώμασι.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 13,
        "verse_text": "Διʼ ἁμαρτίαν χειλέων ἐμπίπτει εἰς παγίδας ἁμαρτωλὸς, ἐκφεύγει δὲ ἐξ αὐτῶν δίκαιος·",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 14,
        "verse_text": "ὁ βλέπων λεῖα ἐλεηθήσεται, ὁ δὲ συναντῶν ἐν πύλαις ἐκθλίψει ψυχάς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 14,
        "verse_text": "Ἀπὸ καρπῶν στόματος ψυχὴ ἀνδρὸς πλησθήσεται ἀγαθῶν, ἀνταπόδομα δὲ χειλέων αὐτοῦ δοθήσεται αὐτῷ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 15,
        "verse_text": "Ὁδοὶ ἀφρόνων ὀρθαὶ ἐνώπιον αὐτῶν, εἰσακούει δὲ συμβουλίας σοφός.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 16,
        "verse_text": "Ἄφρων αὐθημερὸν ἐξαγγέλλει ὀργὴν αὐτοῦ, κρύπτει δὲ τὴν ἑαυτοῦ ἀτιμίαν ἀνὴρ πανοῦργος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 17,
        "verse_text": "Ἐπιδεικνυμένην πίστιν ἀπαγγέλλει δίκαιος, ὁ δὲ μάρτυς τῶν ἀδίκων δόλιος.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 18,
        "verse_text": "Εἰσὶν οἳ λέγοντες τιτρώσκουσι, μάχαιραι· γλῶσσαι δὲ σοφῶν ἰῶνται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 19,
        "verse_text": "Χείλη ἀληθινὰ κατορθοῖ μαρτυρίαν, μάρτυς δὲ ταχὺς γλῶσσαν ἔχει ἄδικον.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 20,
        "verse_text": "Δόλος ἐν καρδίᾳ τεκταινομένου κακὰ, οἱ δὲ βουλόμενοι εἰρήνην εὐφρανθήσονται.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 21,
        "verse_text": "Οὐκ ἀρέσει τῷ δικαίῳ οὐδὲν ἄδικον, οἱ δὲ ἀσεβεῖς πλησθήσονται κακῶν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 22,
        "verse_text": "Βδέλυγμα Κυρίῳ χείλη ψευδῆ, ὁ δὲ ποιῶν πίστεις δεκτὸς παρʼ αὐτῷ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 23,
        "verse_text": "Ἀνὴρ συνετὸς θρόνος αἰσθήσεως, καρδία δὲ ἀφρόνων συναντήσεται ἀραῖς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 24,
        "verse_text": "Χεὶρ ἐκλεκτῶν κρατήσει εὐχερῶς, δόλιοι δὲ ἔσονται ἐν προνομῇ.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 25,
        "verse_text": "Φοβερὸς λόγος καρδίαν ταράσσει ἀνδρὸς δικαίου, ἀγγελία δὲ ἀγαθὴ εὐφραίνει αὐτόν.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 26,
        "verse_text": "Ἐπιγνώμων δίκαιος ἑαυτοῦ φίλος ἔσται, ἁμαρτάνοντας δὲ καταδιώξεται κακὰ, ἡ δὲ ὁδὸς τῶν ἀσεβῶν πλανήσει αὐτούς.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 27,
        "verse_text": "Οὐκ ἐπιτεύξεται δόλιος θήρας, κτῆμα δὲ τίμιον ἀνὴρ καθαρός.",
        "iso-639-2_language_code": "grc"
    },
    {
        "book_title": "μετάφραση των εβδομήκοντα ΠΑΡΟΙΜΙΑΙ 12",
        "book_usfm_code": "PRO",
        "chapter_number": 12,
        "verse_number": 28,
        "verse_text": "Ἐν ὁδοῖς δικαιοσύνης ζωὴ, ὁδοὶ δὲ μνησικάκων εἰς θάνατον.",
        "iso-639-2_language_code": "grc"
    }
]