    },
    {
        "book_title": "Genesis",
        "book_usfm_code": "GEN",
        "chapter_number": "10",
        "verse_number": "26",
        "verse_text": "This Joktan conceived Almodad, and Sheleph, and Hazarmaveth, Jera",
        "iso-639-2_language_code": "eng"
    },
